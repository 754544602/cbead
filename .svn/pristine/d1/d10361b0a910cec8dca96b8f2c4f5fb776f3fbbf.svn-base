var projectBillList={init:function(){projectBillList.getprojectBillListInf();$(".project-bill-tbody").on("click",".project-name",function(){debugger;var projectType=$(this).parents("tr").find("td.project-type").text();var projectCode=$(this).parents("tr").find("td.project-code").text();var projectName=encodeURI(encodeURI($(this).parents("tr").find("td.project-name-td").text()));if(projectType=="选学"||projectType=="委托"){var url="projectSelection.html?projectCode="+projectCode+"&projectName="+projectName;window.location.href=encodeURI(url)}else{var url="projectBreak.html?projectCode="+projectCode+"&projectName="+projectName;window.location.href=encodeURI(url)}});$(".search-btn").click(function(){projectBillList.getprojectBillListInf()})},getprojectBillListInf:function(){var selectParam=$(".selectParam").val();$('.project-bill-page').pageInfo({async:true,pageIndexs:1,pageNum:15,pageSize:10,url:$yt_option.base_path+"finance/projectStatement/lookForAll",type:"post",data:{selectParam:selectParam,startTimeStart:"",startTimeEnd:"",endTimeStart:"",endTimeEnd:"",projectType:"",checkInCountStart:"",checkInCountEnd:"",notReconciliationStart:"",notReconciliationEnd:"",isUserProject:"1"},objName:'data',before:function(){$yt_baseElement.showLoading()},success:function(data){if(data.flag==0){var htmlTbody=$('.project-bill-tbody');var htmlTr='';if(data.data.rows.length>0){$.each(data.data.rows,function(i,v){htmlTr+='<tr><td class="project-code">'+v.projectCode+'</td><td class="project-name-td" style="text-align:left;"><a style="color: #4169E1;" class="project-name">'+v.projectName+'</td><td class="project-type">'+(projectBillList.projectTypeInfo(v.projectType))+'</a></td><td>'+v.projectSell+'</td><td>'+v.projectHead+'</td><td>'+v.startDate+'</td><td>'+v.endDate+'</td><td>'+v.checkInCount+'</td><td>'+v.notReconciliationCount+'</td></tr>'})}else{htmlTr+='<tr style="border:0px;background-color:#fff !important;" ><td colspan="10" align="center" style="border:0px;"><div class="no-data" style="width: 280px;margin: 0 auto;"><img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;"></div></td></tr>'}htmlTbody.html(htmlTr);$yt_baseElement.hideLoading()}else{$yt_baseElement.hideLoading(function(){$yt_alert_Model.prompt("查询失败")})}},isSelPageNum:true })},projectTypeInfo:function(code){if(code==1){return "计划"}else if(code==2){return "委托"}else if(code==3){return "选学"}else if(code==4){return "调训"}else{return ''}}};$(function(){projectBillList.init()});