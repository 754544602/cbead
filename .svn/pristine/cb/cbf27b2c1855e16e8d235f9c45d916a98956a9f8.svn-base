.statistics-top .calendar {
	height: 240px !important;
}

.statistics-top {
	width: 100%;
	min-width: 1121px;
	height: 46px;
	box-sizing: border-box;
	padding: 13px 0 9px 16px;
	background-color: #363845;
	box-shadow: inset 0px -1px 0px 0px #5c607a;
}

#txtDate-box {
	text-align: center;
	width: 100%;
	margin-top: -40px;
}

.statistics-top .txtDate-span {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #cccccc;
	cursor: pointer;
	line-height: 46px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

.statistics-top .txtDate-now {
	font-family: MicrosoftYaHei-Bold;
	font-size: 24px;
	letter-spacing: 0px;
	color: #ffffff;
	margin: 0 30px;
}

.statistics-draw1,
.statistics-draw2,
.statistics-draw3 {
	height: 400px;
}

.statistics-box {
	width: 100%;
	min-width: 1121px;
	box-sizing: border-box;
	border: 5px solid #FAFAFA;
	background-color: #FFFFFF;
	position: relative;
}

.statistics-box .average {
	position: absolute;
	top: 20px;
	right: 40px;
	z-index: 100;
}

.statistics-box:nth-child(4) #txtDate-MM {
	position: absolute;
	left: 227px;
	top: 24px;
	z-index: 100;
}

.statistics-box:nth-child(4) .calendar {
	position: absolute;
	top: 48px !important;
	margin-left: 167px;
	height: 200px !important;
}

.statistics-box:nth-child(4) .calendar-box {
	position: absolute !important;
}

.statistics-box .userCount {
	float: right;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
	margin-left: 28px;
}

.statistics-box .userCount-label {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #de595a;
}

.statistics-box:nth-child(5) {
	padding: 10px;
}
.statistics-box:nth-child(5) .draw,.statistics-box:nth-child(5) .left-table{
	width:50% ;
	height: 438px;
	float: left;
	box-sizing: border-box;
}
.statistics-box:nth-child(5) .left-table table{
	width: 100%;
}
.statistics-box:nth-child(5) .left-table table th{
	border: none;
}
.statistics-box:nth-child(5) .left-table table th:nth-child(1),
.statistics-box:nth-child(5) .left-table table td:nth-child(1){
	text-align: left;
	padding-left: 7px;
}
.statistics-box:nth-child(5) .left-table table td:not(:nth-child(1)){
	text-align: center;
}
.statistics-box:nth-child(5) .left-table table tr{
	border-bottom: 1px solid #e6e6e6;
	height: 42px;
}
.statistics-box:nth-child(5) .statistics-draw4{
	height:430px ;
}
.statistics-box:nth-child(5) h1 {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #111111;
}
.statistics-draw5{
	height: 270px;
}
