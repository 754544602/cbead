@charset "utf-8";
#newDemandApply {
	padding-top: 20px;
	margin: 5px;
	border: 1px solid #DFE6F3;
	background-color: #FFFFFF;
	min-width: 1250px;
}

.form-num-model {
	float: right;
	display: inline;
	padding-right: 35px;
	color: #384495;
	font-weight: bold;
}

#formNum,
#applyDate {
	font-size: 14px;
	font-weight: normal;
}

table td {
	position: relative;
}

.base-form-info {
	padding-top: 10px;
	/*padding: 15px 10px 0px 5px;*/
}

.base-form-info table tr td {
	padding: 10px 0px;
}

.firstRow .firstRow td {
	width: 300px;
}

.boldL {
	color: #384495;
	font-weight: bold;
}

.firstTr .selectBox {
	float: left;
}

.firstTr .selectBox .yt-select {
	width: 120px;
	margin-right: 20px;
}


/*
 * 采购信息样式
 */

.firstTr .selcetBox2 {
	width: 150px;
}

#loanReason {
	width: 97%;
}

#returnDate {
	width: 150px;
}

.valid-font1 {
	position: absolute;
	top: 28px;
	left: 100px;
}

.valid-font2 {
	position: absolute;
	top: 28px;
	left: 100px;
}

.valid-font3 {
	position: absolute;
	top: 28px;
	left: 140px;
}

.valid-font4 {
	position: absolute;
	top: 28px;
	left: 65px;
}

.thing-td {
	width: 260px;
}

.form-btn-group {
	padding: 40px 0px 30px 0px;
	text-align: center;
}

.form-btn-group button {
	height: 40px;
	line-height: 38px;
	font-size: 18px;
}


/*
 * 
 * 审批流程
 * 
 */

#flowApproveTab button {
	min-width: 100px;
	height: 40px;
	line-height: 38px;
	font-size: 18px;
}

#flowApproveTab tr td {
	padding-top: 20px;
}

.flow-approve-model {
	padding: 8px 10px 0px 10px;
}

.operate-flow-sel,
.approve-users-sel {
	width: 220px;
}

#operateMsg {
	width: 96.2%;
	height: 42px;
}

#ppsProject {
	width: 300px;
	cursor: pointer;
}

#approveSaveBtn {
	margin-right: 55px;
}

#approveSubBtn {
	margin-right: 55px;
}

.flow-textare-risk {
	position: absolute;
	bottom: 5px;
}

@media screen and (min-width: 1366px) and (max-width:1440px) {
	/*当屏幕尺寸大于1366,小于1440px时，应用下面的CSS样式*/
	.dept-label {
		padding-left: 180px;
	}
	.job-label {
		padding-left: 248px;
	}
	.tel-label {
		padding-left: 210px;
	}
	.prj-td {
		padding-left: 150px !important;
	}
	.retu-date {
		padding-right: 283px;
	}
	.thing-td {
		width: 350px;
	}
}


