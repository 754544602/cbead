.before-apply-form-model {
	position: fixed;
	top: 0;
	left: 0;
	z-index: 10001;
	width: 572px;
	display: none;
}

#beforeApplyFormModel .heard-search-model button {
	margin-bottom: 2px;
}

#aproSearchBtn {
	margin-right: 5px;
}

#keywordInpu {
	width: 406px;
	margin-right: 5px;
}

#beforeApplyFormModel .model-bottom-btn {
	padding: 25px 0px;
	text-align: center;
}

#aproSureBtn {
	margin-right: 35px;
}

#beforeApplyFormModel .heard-search-model {
	padding-bottom: 18px;
}

#beforeApplyFormModel .model-content {
	padding: 20px 20px 0px 20px;
}

#beforeApplyFormModel .table-page td>div {
	padding: 0 4px !important;
}


/**
 * 
 * 
 * 所属项目
 * 
 */

#fromPrjModel {
	width: 572px;
	display: none;
}

#fromPrjModel .heard-search-model button {
	margin-bottom: 2px;
}

#prjSearchBtn {
	margin-right: 5px;
}

#prjKeyWord {
	width: 407px;
	margin-right: 5px;
}

.money-quality-sel {
	width: 337px;
}

#fromPrjModel .model-bottom-btn {
	padding-top: 25px;
	text-align: center;
}

#prjSureBtn {
	margin-right: 35px;
}

#fromPrjModel .heard-search-model {
	padding-bottom: 18px;
}

#fromPrjModel .model-content {
	padding: 20px 18px;
}

#fromPrjModel .table-page td>div {
	padding: 0 4px !important;
}

#fromPrjModel .appro-list-model {
	width: 535px;
}

.base-form-info tr td:first-child {
	text-align: right;
	padding-right: 2px;
}

.qtip-default {
	display: none;
}

.base-form-info td {
	text-align: left !important;
	padding-left: 10px !important;
	padding-top: 5px !important;
}

.life-of-loan-sel {
	width: 180px;
}

.refund-tab-inp {
	width: 95%;
	padding-right: 5px;
	text-align: right;
}

.yt-edit-alert {
	width: 700px;
}

.yt-edit-alert-main {
	padding: 10px;
}

.yt-eidt-model-bottom {
	border: 0 !important;
}

.moneyText {
	margin-right: 5px;
	text-align: right;
}

.secondary-back {
	position: relative;
	width: 6px;
	height: 18px;
	margin-top: 8px;
	background-color: #244972;
	float: left;
	margin-left: 20px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}

.secondary-title {
	font-size: 16px;
	height: 35px;
	line-height: 35px;
	border-left: 0;
	border-right: 0;
	padding-left: 45px;
	color: #384495;
	font-weight: bold;
}

.file-box .li-div {
	position: relative;
	display: block !important;
	color: #384495;
}

.file-name {
	color: #333;
}

.file-box .li-div .file-dw {
	position: relative;
	background: url(../../../../../images/common/down-load-icon.png) no-repeat left;
	padding-left: 20px;
	cursor: pointer;
	color: #4c4c4c;
	float: right;
	margin-right: 50px;
	width: 30px;
}

.file-box .li-div .file-pv {
	position: relative;
	background: url(../../../../../images/common/preview-icon.png) no-repeat left;
	padding-left: 20px;
	margin-right: 50px;
	color: #4c4c4c;
	float: right;
}

.file-box .li-div .file-pv img {
	position: absolute;
	left: 0;
	opacity: 0;
	width: 100%;
	height: 100%;
	cursor: pointer;
}

.file-box .li-div .file-pvno {
	position: relative;
	background: url(../../../../../images/common/preview-icon.png) no-repeat left;
	padding-left: 20px;
	margin-right: 50px;
	float: right;
	color: #ccc;
}

.metaphase-table {
	width: 100%;
	table-layout: auto;
	margin-bottom: 20px;
}
.metaphase-table td{
	text-align: center;
}
.year-table {
	width: 100%;
	table-layout: auto;
	margin-bottom: 20px;
}
.year-table td{
	text-align: center;
}
.base-form-metaphase table tr td {
    padding: 10px 0px;
    padding-left: 10px;
    padding-top: 5px;
}
.button-box {
    padding: 10px 0;
    text-align: center;
}
.button-box button {
    height: 40px;
    line-height: 40px;
    padding: 0 30px;
    font-size: 18px;
    border-radius: 2px;
    cursor: pointer;
    outline: none;
}
