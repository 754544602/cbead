var noSetProjectList={init:function(){$(".chose-year").calendar({speed:200,complement:true,readonly:true,lowerLimit:"NaN",nowData:true,dateFmt:"yyyy",callback:function(){noSetProjectList.getNoProjectApprovalList()}});noSetProjectList.getNoProjectApprovalList();$('.search-btn').click(function(){noSetProjectList.getNoProjectApprovalList()});$(".noSetProject-tbody").on('click',".file-name",function(){var pkId=$('.yt-table-active .pkId').val();var projectCode=$('.yt-table-active .project-code-list').val();var projectState=$(this).parents('tr').find(".project-state-step").val();var projectType=$(this).parents('tr').data('data').projectType;window.location.href="/cbead/website/view/project/projectDetails.html?pkId="+pkId+"&tp="+1+"&projectCode="+projectCode+'&projectState='+projectState+"&projectType="+projectType})},getNoProjectApprovalList:function(){$yt_baseElement.showLoading();var selectParam=$(".select-param").val();var years=$(".chose-year").val();$('.noSetProject-page').pageInfo({pageIndexs:1,pageNum:15,pageSize:10,url:$yt_option.base_path+"project/lookForAllByProjectNoPass",type:"post",data:{selectParam:selectParam,years:years},objName:'data',success:function(data){$yt_baseElement.showLoading();if(data.flag==0){var htmlTbody=$('.noSetProject-tbody').empty();var htmlTr='';if(data.data.rows.length>0){$.each(data.data.rows,function(i,v){if(v.projectType==2){v.projectTypeVal="委托"}else if(v.projectType==3){v.projectTypeVal="选学"}else if(v.projectType==4){v.projectTypeVal="调训"}if(v.projectStates==1){v.projectStatesVal="未立项"}else if(v.projectStates==2){v.projectStatesVal="审批中"}else if(v.projectStates==3){v.projectStatesVal="未通过"}else if(v.projectStates==4){v.projectStatesVal="已取消"}htmlTr='<tr><td style="width:50px;word-wrap:break-word;"><input type="hidden" value="'+v.pkId+'" class="pkId">'+(i+1)+'</td><td style="text-align:left"><a style="color:#3c4687;" class="file-name"><input type="hidden" value="'+v.projectCode+'" class="project-code-list">'+v.projectName+'</a></td><td>'+v.projectTypeVal+'</td><td>'+v.startDate+'</td><td style="text-align:right">'+v.trainDateCount+'</td><td style="text-align:left">'+v.projectSell+'</td><td style="text-align:right">'+v.traineeCount+'</td><td><input type="hidden" value="'+v.projectStates+'" class="project-state-step">'+v.projectStatesVal+'</td></tr>';$(".noSetProject-page").show();htmlTr=$(htmlTr).data('data',v);htmlTbody.append(htmlTr)})}else{$(".noSetProject-page").hide();htmlTr='<tr style="border:0px;background-color:#fff !important;" ><td colspan="8" align="center" style="border:0px;"><div class="no-data" style="width: 280px;margin: 0 auto;"><img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;"></div></td></tr>';htmlTbody.append(htmlTr)}$yt_baseElement.hideLoading()}else{$yt_baseElement.hideLoading(function(){$yt_alert_Model.prompt("查询失败")})}},isSelPageNum:true })}};$(function(){noSetProjectList.init()});