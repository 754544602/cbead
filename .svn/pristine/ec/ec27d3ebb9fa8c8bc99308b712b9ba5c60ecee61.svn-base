								<tr>
									<th class="check-box-th" style="width: 30px;">
										<label class="check-label yt-checkbox parent-check" style="margin-left: 7px;"><input type="checkbox" class="check-all" name="test" value=""/>  </label>
									</th>
									<th style="width: 60px;">分组</th>
									<th style="width: 90px;">姓名</th>
									<th style="width: 60px;">房间号</th>
									<th style="width: 120px;">手机号</th>
									<th style="text-align: left;">集团</th>
									<th style="text-align: left;">单位</th>
									<th style="width: 80px;">开票信息</th>
									<th style="width: 85px;">报到状态</th>
									<th style="width: 85px;">对账状态</th>
									<th style="width: 75px;">开票状态</th>
									<th style="width: 110px;">备注</th>
								</tr>
							</thead>
							<tbody class="yt-tbody list-tbody class-tbody">
								<tr>
									<td colspan="12" align="center" style="border:0px;">
										<div class="no-data" style="width: 280px;margin: 0 auto;">
											<img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;">
										</div>
									</td>
								</tr>
							</tbody>
						</table>
						<div class="page-info class-page" onselectstart="return false"></div>
					</div>
				</div>
				<!--数据展示区域END-->
				
				<!--修改备注STAR-->
				<div class="yt-pop-model yt-edit-alert edit-remark-alert" style="width: 400px;"> 
					<div class="yt-edit-alert-title">  
		                <span class="yt-edit-alert-title-msg">开票信息</span>  
		          </div>  
		          <!--主体内容部分START-->  
		          <div class="yt-edit-alert-main">  
		            <form style=" overflow: auto;" class="alert-form">
						<div class="tab-content">
							<div class="class-info-centent">
				                <table class="edit-remark" style="margin: 0 auto;">  
				                <tr>
				                	<td style="vertical-align: top;">备注：</td>
				                	<td>
				                		<textarea class="remark-text" style="resize: none;min-height: 50px;width: 300px;"></textarea>
				                	</td>
				                </tr>
				                </table> 
				            </div>
				        </div> 
		            </form>  
		            <!--底部按钮START-->  
		            <div class="yt-eidt-model-bottom remark-btn-box">  
		                <input style="border-radius: 3px;" class="yt-model-bot-btn yt-model-sure-btn remark-sure-btn" type="button" value="确定" />
						<input style="border-radius: 3px;" class="yt-model-bot-btn yt-model-canel-btn remark-canel-btn" type="button" value="取消" />
		            </div>  
		          </div>  
		          <!--主体内容部分END-->  
		    </div>
