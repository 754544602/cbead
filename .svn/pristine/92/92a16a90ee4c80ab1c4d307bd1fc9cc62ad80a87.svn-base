<!DOCTYPE html>
<html>

	<head>
		<meta http-equiv="x-ua-compatible" content="ie=Edge" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<!--样式文件引用-->
		<link rel="stylesheet" type="text/css" href="../../resources/js/scroll/jquery.mCustomScrollbar.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/common/yt-common.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/page/yt-page.min.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/modules/teacher/teacherList.css" />
		<!--日期控件css-->
		<link rel="stylesheet" type="text/css" href="../../resources/css/calendar/yt-calendar.min.css" />
		<title>我的项目预算</title>
		<style type="text/css">
			.calendar{
				z-index: 10000 !important;
			}
		</style>
	</head>
	<body>
		<div id="project-main" style="min-width: 1100px;">
			<!--顶部条件查询START-->
			<div class="head-div" style="height: 20px;">
				<div class="search-btn-slect">
					<div class="">
						<button class="yt-option-btn senior-search-btn" clickstate="true" style="margin: 0px 10px;float: right;">
							更多
							<img class="search-put" src="../../resources/images/icons/class-put.png">
						</button>
						<div class="key-word" style="margin-right: 0px;">
							<input class="yt-input selectParam" placeholder="项目编号，项目名称，项目主任" type="text"/>
							<button class="yt-option-btn search-btn">
								<img class="search-img" src="../../resources/images/icons/class-search.png" />
							</button>
						</div>
						<!--弹出START-->
					<div class="search-box" style="width:390px;right: 16px;">
					<table class="search-table">
						<tr>
							<td>项目编号：</td>
							<td>
								<input class="yt-input project-code" style="width: 304px;" type="text" placeholder="请输入" />
							</td>
						</tr>
						<tr>
							<td>项目名称：</td>
							<td>
								<input class="yt-input project-name" style="width: 304px;" type="text" placeholder="请输入" />
							</td>
						</tr>
							<tr>
							<td>项目主任：</td>
							<td>
								<input class="yt-input project-head" style="width: 304px;" type="text" placeholder="请输入" />
							</td>
						</tr>
						<tr>
							<td>开班日期：</td>
							<td>
								<input class="calendar-input start-time-start" style="width: 134px;" value="" placeholder="请选择" type="text" />
								<span class="date-line">—</span>
								<input class="calendar-input start-time-end" style="width: 134px;" value="" placeholder="请选择" type="text" />
							</td>
						</tr>
						<tr>
							<td>申请时间：</td>
							<td>
								<input class="calendar-input end-time-start" style="width: 134px;" value="" placeholder="请选择" type="text" />
								<span class="date-line">—</span>
								<input class="calendar-input end-time-end" style="width: 134px;" value="" placeholder="请选择" type="text" />
							</td>
						</tr>
						<tr>
							<td>培训天数：</td>
							<td>
								<input class="yt-input train-start" style="width: 138px;text-align: right;" value="" placeholder="请输入" type="number" />
								<span class="date-line">—</span>
								<input class="yt-input train-end" style="width: 138px;text-align: right;" value="" placeholder="请输入" type="number" />
							</td>
						</tr>
						<tr>
							<td>预算总额：</td>
							<td>
								<input class="yt-input budge-start" style="width: 138px;text-align: right;" value="" placeholder="请输入" type="number" />
								<span class="date-line">—</span>
								<input class="yt-input budge-end" style="width: 138px;text-align: right;" value="" placeholder="请输入" type="number" />
							</td>
						</tr>
						<!--<tr>
							<td>项目状态：</td>
							<td>
								<div class="base-form-model select-type">
									<div class="project-state-box">
										<select id="project-states" class="yt-select"  style="width: 311px;">
											<option value="">请选择</option>
											<option value="2">培训中</option>
											<option value="3">未结项</option>
											<option value="4">已结项</option>
										</select>
									</div>
								</div>
							</td>
						</tr>-->
						<tr>
							<td>执行率：</td>
							<td>
								<input class="yt-input imp-start" style="width: 137px;text-align: right;" value="" placeholder="请输入" type="number" />
								<span class="date-line">—</span>
								<input class="yt-input imp-end" style="width: 137px;text-align: right;" value="" placeholder="请输入" type="number" />
							</td>
						</tr>
						<tr>
							<td>剩余预算：</td>
							<td>
								<input class="yt-input surpl-start" style="width: 137px;text-align: right;" value="" placeholder="请输入" type="number" />
								<span class="date-line">—</span>
								<input class="yt-input surpl-end" style="width: 137px;text-align: right;" value="" placeholder="请输入" type="number" />
							</td>
						</tr>
					</table>
					<!--底部按钮START-->
					<div class="search-bottom-btn">
						<button class="yt-model-bot-btn yt-model-sure-btn">
							<img class="search-btn-img" src="../../resources/images/icons/class-search.png"/>
						</button>
						<button class="yt-model-bot-btn yt-model-reset-btn" style="outline: none;cursor: pointer;">
							<img class="search-btn-img" src="../../resources/images/icons/class-reset.png"/>
						</button>
					</div>
				</div>
				<!--弹出END-->
					</div>
					</div>
				</div>
			<!--顶部条件查询END-->
			<!--数据展示区域START-->
			<div class="list-box">
				<div id="" style="padding: 12px 0px 12px 0px;">
				项目预算查询
				</div>
					<table class="yt-table list-table">
						<thead class="list-thead">
							<tr>
								<th style="width: 70px;">项目编号</th>
								<th>项目名称</th>
								<th style="width: 120px;">项目主任</th>
								<th width="140px">开班日期</th>
								<th style="width: 70px;">培训天数</th>
								<th width="120px">申请时间</th>
								<th style="width: 90px;">预算总额</th>
								<th style="width: 100px;">状态</th>
								<th style="width: 80px;">执行率(%)</th>
								<th style="width: 90px;">剩余预算</th>
							</tr>
						</thead>
						<tbody class="yt-tbody budget-query-tbody list-tbody">
							<tr class="class-tr">
								<td colspan="10" align="center" style="border:0px;">
									<div class="no-data" style="width: 280px;margin: 0 auto;">
										<img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;">
									</div>
								</td>
							</tr>
						</tbody>
					</table>
					<div class="page-info budget-query-page" onselectstart="return false"></div>
			</div>
			<!--数据展示区域END-->
		</div>
	</body>
	<!--js文件引用-->
	<script type="text/javascript" src="../../resources/js/common/jquery.min.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-option.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-common.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-frame.min.js"></script>
	<script type="text/javascript" src="../../resources/js/dotdotdot/jquery.dotdotdot.min.js"></script>
	<script type="text/javascript" src="../../resources/js/page/yt-page.min.js"></script>
	<script type="text/javascript" src="../../resources/js/valid/yt-valid.min.js"></script>
	<!--滚动条js-->
	<script type="text/javascript" src="../../resources/js/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
	<!--日期控件js-->
	<script type="text/javascript" src="../../resources/js/calendar/yt-calendar.min.js"></script>
	<script type="text/javascript" src="../../resources/js/fileUpload/ajaxfileupload.js"></script>
	<script type="text/javascript" src="../../resources/js/modules/budgetQuery/budgetQueryList.js"></script>
</html>