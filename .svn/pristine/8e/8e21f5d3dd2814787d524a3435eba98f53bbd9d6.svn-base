.every-part {
	border-radius: 10px;
}

.every-part:nth-child(1),
.every-part:nth-child(2) {
	width: 50%;
	height: 400px;
	float: left;
	box-sizing: border-box;
	padding: 10px;
	border: 5px solid #FAFAFA;
	background-color: #FFFFFF;
	position: relative;
}

.every-part:nth-child(1) label {
	position: absolute;
	z-index: 100;
	right: 30px;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
}

.every-part:nth-child(1) span {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #de595a;
}

.every-part:nth-child(2) span {
	position: absolute;
	left: 47%;
	top: 48%;
	z-index: 100;
	font-size: 18px;
	letter-spacing: 1px;
	color: #111111;
}

.every-part .average{
	position: absolute;
	top: 20px;
	right: 40px;
	z-index: 100;
}

.every-part .userCount{
	float: right;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
	margin-left: 28px;
}

.every-part .userCount-label{
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #de595a;
}

.every-part:nth-child(4),
.every-part:nth-child(5),
.every-part:nth-child(6),
.every-part:nth-child(7),
.every-part:nth-child(8),
.every-part:nth-child(9),
.every-part:nth-child(10) {
	border: 5px solid #FAFAFA;
	padding: 10px 10px 70px 10px;
	box-sizing: border-box;
	background-color: #FFFFFF;
	position: relative;
}