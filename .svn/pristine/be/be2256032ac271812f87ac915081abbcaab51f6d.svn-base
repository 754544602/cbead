.project-title-name {
	width: 100%;
	height: 18px;
	font-family: MicrosoftYaHei;
	font-size: 18px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #111111;
	margin: 17px 0px 15px 0px;
	text-align: center;
}
.btn-return {
	position: absolute;
    top: 0;
    right: 4px;
    height: 45px;
    background: none;
    border: none;
    color: #cccccc;
}
.project-title-list {
	width: width;
	height: 51px;
	background-color: #ffffff;
	border-radius: 5px;
	border: solid 4px rgba(245, 245, 245, 0.5);
	margin: 0px 0px 6px 5px;
	text-align: center;
}

.class-info-icon1 {
	position: absolute;
}

.class-info-icon2 {
	margin: 10px 0px -8px 0px;
}

.class-info-name {
	margin-left: 15px;
	border-bottom: solid 1px #6495BC;
}
.btn-file{
	position: relative;
    width: 72px;
    height: 25px;
    margin-left: 125px;
    border: none;
    background: #6495bc;
    color: #FFFFFF;
    border-radius: 2px;
}
.fileIds{
    width: 70px;
	position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    z-index: 1000;
    font-size: 20px;
    cursor: pointer;
}
.teacher-index{
	margin: 0 auto;
}
.teacher-index td{
	height: 50px;
}
.tab-title-list{
	height: 36px;
}
.tab-title-list button.active{
	color: #fff;
	background: url(../../../images/icons/class-list.png) no-repeat;
}
.box-list{
	margin: 0 auto;
}
.teacher-index-inf td{
	height: 50px;
}
.teacher-index-inf table input{
	border: none;
}
.class-table{
	width: 100%;
}
.img-amend,.img-del{
	position: relative;
	top: 3px;
}
.img-amend{
	right: 5px;
}
.img-del{
	left: 5px;
}
.alert-index{
	width: 600px;
	height: 315px;
}
.course-title,.course-details{
	width: 420px;
}
.course-details{
	height: 128px;
}
.teacher-index-inf{
	margin: 0 auto;
}
.yt-eidt-model-bottom{
	margin-top: 12px;
}
.project-title-div{
	position: relative;
    height: 45px;
    line-height: 45px;
    background: #363845;
    border-bottom: 1px solid #5c607a;
}
.tip-box:before {
	content: '';
	display: inline-block;
	position: absolute;
	height: 12px;
	width: 5px;
	background: #968deb;
	top: 9px;
	left: 0;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}
.tip-box {
	color: #fff;
	padding: 0 12px;
	position: relative;
}
.tip-table td.tip-lable {
	text-align: right;
}

.tip-table td {
	color: #fff;
	padding: 5px 0;
}