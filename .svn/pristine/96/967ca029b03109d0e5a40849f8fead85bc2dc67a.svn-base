@charset "utf-8";
#reimApproList {
    margin: 5px;
    min-width:1100px;
    border: 1px solid #DFE6F3;
    background-color: #FFFFFF;
}

.head-div {
	padding: 20px 10px 17px 10px;
}

#heardSearchBtn {
	margin:0px 5px 3px 12px;
	padding-bottom: 0;
	line-height: 28px;
}
#scanSearch{
	float: right;
	display: inline;
}
.head-div .search {
	width: 210px;
	padding-right: 25px;
}

.head-div .yt-option-btn {
	vertical-align: initial;
}
.tab-header {
	border-top: 1px solid #DFE6F3;
	background: #EFF3F6;
	height: 40px;
}

.tab-header ul li {
	float: left;
}

.tab-li {
	width: 100px;
	height: 40px;
	line-height: 38px;
	font-size: 16px;
	text-align: center;
	cursor: pointer;
	box-sizing: border-box;
	border-top: 1px solid #EFF3F6;
	border-left: 1px solid #EFF3F6;
	border-right: 1px solid #EFF3F6;
}

.active-li {
	border-top: 1px solid #de595a;
	border-left: 1px solid #DFE6F3;
	border-right: 1px solid #DFE6F3;
	background: #FFFFFF;
	box-sizing: border-box;
}

.tab-content {
	position: relative;
}

.tab-content .yt-table {
	width: 100%;
}

.redSign {
	vertical-align: middle;
	margin-right: 4px;
	color: red;
}

.prjNameLayout {
	overflow: hidden;
	max-height: 40px;
	line-height: 20px;
	text-align: left;
	word-break: break-all;
}

.flowWindow {
	width: 650px;
	height: 420px;
}


.flowBtn {
	color: #384495;
}
.imgBox{
	overflow: auto;
	height: 380px;
}


#pro-proce {
	width: 100%;
	position: relative;
	padding: 24px 0 22px 0;
}

#pro-proce div {
	width: 95%;
	margin: 0 auto;
	position: relative;
	border-left: 1px solid #999;
}

#pro-proce div img {
	position: absolute;
	top: 0;
	left: -5px;
}

#pro-proce ul {
	width: 100%;
	height: 40px;
	line-height: 40px;
	padding: 10px auto;
	margin-left: 20px;
	position: relative;
	top: -15px;
}

#pro-proce ul li {
	float: left;
	color: #418dcc;
}

.qtip-content {
	padding: 5px 9px;
	padding-bottom: 0px;
	text-align: left;
	max-height: 500px;
	word-wrap: break-word;
	overflow: auto !important;
}

.suspension {
	width: 380px;
	height: auto;
	position: relative;
	padding: 10px;
	padding-bottom: 0px;
}

.susp-all div {
	float: left;
}

.suspension div {
	/*  width: 308px;
    border-left: 1px solid #DFE6F3;
    position: relative;*/
}

.susp-img {
	position: absolute;
	left: -5px;
	top: 0px;
}

.suspension div ul {
	width: 100%;
	margin-left: 15px;
	position: relative;
	top: -6px;
}

.suspension div ul li {
	height: 20px;
	line-height: 20px;
}

.susp-three .susp-img {
	left: -4px;
}

.susp-all {
	width: 100% !important;
}

.susp-name {
	border: none !important;
	width: 70px !important;
	color: #333333;
	margin-top: -3px;
}

.susp-all:last-child .susp-one {
	border: none;
	padding-bottom: 0 !important;
}

.clearImg {
	display: none;
	position: absolute;
	left: 290px;
	top: 34px;
	cursor: pointer;
}

tr.yt-table-active td {
	border-right-color: #fff;
}


/**
 * 2017.12.5
 */

#project-main {
	border: 1px solid #DFE6F3;
	background-color: #FFFFFF;
	margin: 5px;
	min-width: 1100px;
}

.center-line {
	margin: 0px 10px;
	color: #384495;
}


/**
 * 2017.12.07
 */
.page-info {
	margin-top: 15px !important;
}

