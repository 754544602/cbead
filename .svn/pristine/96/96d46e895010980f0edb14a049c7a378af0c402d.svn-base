.programme .list-title {
	height: 56px;
	line-height: 56px;
}

.border-table {
	width: 100%;
}

.border-table td,
.border-table th {
	border: 1px solid #e2e9ef;
	padding: 5px;
	text-align: left;
	color: #333333;
	position: relative;
}

.programme th {
	position: relative;
	color: #888888;
	padding: 8px 5px;
}

.programme thead tr th span {
	position: absolute;
	top: 0;
	right: 0;
	text-align: right;
	font-size: 12px;
	display: inline-block;
	padding: 2px 2px 0 0;
	width: 58px;
	height: 33px;
	background: url(../../../../resources/images/img/day-head.png) no-repeat;
	color: #4c4c4c;
}

.border-table .have-couse {
	color: #fff;
	text-align: center;
}

.programme th.state-day div {
	width: 115px;
}

.morning .have-couse {
	position: relative;
	overflow: hidden;
}

.morning .have-couse .teacher-name {
	background: #f59700;
}

.afternoon .have-couse .teacher-name {
	background: #b8b3e2;
}

.night .have-couse .teacher-name {
	background: #847ccf
}

.programme th.state-day {
	text-align: left;
	text-indent: 15px;
}

.programme .tool-box {
	position: absolute;
	width: 101%;
	top: auto;
	left: 0;
	height: 22px;
	margin-top: -11px;
}

.have-couse .teacher-name {
	position: absolute;
	height: 22px;
	left: 0;
	top: auto;
	text-align: center;
	line-height: 22px;
	width: 100%;
	margin-top: -11px;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 2px;
}

.programme .tool-info {
	width: 115px;
	height: 22px;
	float: left;
	cursor: pointer;
}

.content-box.programme {
	margin: 0 5px;
	padding: 0 6px;
}

.table-title {
	height: 40px;
	text-align: center;
	line-height: 40px;
}

.title-h1 {
	font-family: MicrosoftYaHei;
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0;
	color: #111111;
	margin: 6px 0 11px;
}