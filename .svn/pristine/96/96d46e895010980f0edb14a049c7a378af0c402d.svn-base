.border-table {
	width: 100%;
}

.border-table td,
.border-table th {
	border: 1px solid #e2e9ef;
	padding: 5px;
	text-align: left;
	color: #333333;
	position: relative;
}

.border-table th {
	color: #888888;
	background: none;
}

.list-thead th.sun-head {
	background: url(../../../images/img/sun.png) no-repeat;
}

.list-thead th.sat-head {
	background: url(../../../images/img/sat.png) no-repeat;
}

.border-table th.state-day {
	text-align: center;
	width: 1px;
	padding: 0;
}

.border-table th.state-day div {
	width: 28px;
}
.state-day.all-use:after,
.state-day.sub-use:after,
.state-day.base-use:after {
	content: '';
	position: absolute;
	display: inline-block;
	top: auto;
	left: -1px;
	width: 100%;
	height: 22px;
	padding: 0 1px;
	margin-top: -11px;
	background: #f59700;
}

.state-day.all-use:after {
	background: #847ccf;
}
.state-day.base-use:after{
	background: #de595a;
}
.state-day.all-use.start-state:after,
.state-day.sub-use.start-state:after,
.state-day.base-use.start-state:after {
	-webkit-border-radius: 5px 0 0 5px;
	-moz-border-radius: 5px 0 0 5px;
	border-radius: 5px 0 0 5px;
}

.state-day.all-use.end-state:after,
.state-day.sub-use.end-state:after,
.state-day.base-use.end-state:after {
	-webkit-border-radius: 0 5px 5px 0;
	-moz-border-radius: 0 5px 5px 0;
	border-radius: 0 5px 5px 0;
}

.state-day.all-use.end-state.start-state:after,
.state-day.sub-use.end-state.start-state:after,
.state-day.base-use.end-state.start-state:after {
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

.tip-table td.tip-lable {
	text-align: right;
	color: #968deb;
}

.tip-table td {
	color: #fff;
	padding: 5px 0;
}

.calendar .calMain .calContent .enabled {
	height: 80px;
}

.mouth-check {
	cursor: pointer;
	color: #7b74c7;
}

.tip-box {
	border-top: 1px dashed #ffffff;
	color: #fff;
	padding: 0 12px;
	position: relative;
}

.tip-box:first-child {
	border: none;
}

.tip-box table td.tip-lable {
	color: #FFF;
}

.tip-box:before {
	content: '';
	display: inline-block;
	position: absolute;
	height: 12px;
	width: 5px;
	background: #968deb;
	top: 9px;
	left: 0;
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}

.content-box {
	background: #FFFFFF;
}

.change-week span {
	color: #7b74c7;
	font-size: 16px;
	margin: 0 15px;
	cursor: pointer;
}

.example span {
	position: relative;
	color: #555555;
	font-size: 12px;
	margin: 0 35px;
}

.example span:after {
	position: absolute;
	left: -14px;
	top: 5px;
	width: 8px;
	height: 8px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	content: '';
	display: inline-block;
	background: #b8b3e2;
}

