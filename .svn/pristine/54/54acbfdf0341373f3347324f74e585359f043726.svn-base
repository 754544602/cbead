.class-info-name{
	position: relative;
	font-size: 18px;
	color: #6495bc;
	font-weight: bold;
}
.discuss-info-div{
	position: relative;
}
.discuss-steps{
    position: relative;
    top: 6px;
    right: 11px;
}
.steps-num{
	color: #6495bc;
	width: 32px;
	height: 32px;
	font-size: 18px;
    line-height: 32px;
    text-align: center;
	font-weight: bold;
	position: absolute;
	top: 6px;
	left: 4px; 
	z-index: 2;
}
.discuss-btn{
	float: right;
	margin: 8px 40px 0px 0px;
}
.discuss-btn button{
	margin-right: 10px;
}
.discuss-box table span{
	font-size: 14px;
}
.offer-case{
	color: #6495bc;
}
.btn-add{
	width: 73px;
	margin: 0 auto;
}
.add-discuss{
	width: 72px;
}
.discuss-info-table{
    margin: 25px auto;
}
