body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,xmp,pre,code,form,fieldset,legend,input,button,textarea,p,blockquote,th,td { margin:0; padding:0; }
body { background:#f3f3f3; color:#333; font-size:14px; font-family: Microsoft YaHei,Verdana, Arial, Helvetica, sans-serif; }
td,th,caption { font-size:14px; }
h1, h2, h3, h4, h5, h6 { font-weight:normal; font-size:100%; }
address, caption, cite, code, dfn, em, strong, th, var { font-style:normal; font-weight:normal;}
a { color:#555; text-decoration:none; }
a:hover { color:#2080bf;text-decoration:underline; }
img,tr,td{ border:none; }
ol,ul,li { list-style:none; }
input, textarea, select, button { font:14px Microsoft YaHei,Verdana, Arial, Helvetica, sans-serif; }
table { border-collapse:collapse; border: none;}
select{
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
}
.yt-option-button-sure{background: #3C7AB9 !important;}
/*清除ie的默认选择框样式清除，隐藏下拉箭头*/
select::-ms-expand { display: none; }
/*隐藏ie输入框自带x样式*/
::-ms-clear{display: none;}
/*隐藏ie输入框密码自带小眼睛查看样式*/
::-ms-reveal{display: none;}
/*placeholder 字体颜色*/
::-webkit-input-placeholder { /* WebKit browsers */ 
color: #999 !important; 
} 
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */ 
color: #999 !important; 
} 
::-moz-placeholder { /* Mozilla Firefox 19+ */ 
color: #999 !important; 
} 
:-ms-input-placeholder { /* Internet Explorer 10+ */ 
color: #999 !important; 
}
.clear-both{
	clear: both;
}
input::-webkit-input-placeholder, textarea::-webkit-input-placeholder {
　color: #999 !important;
}
input:-moz-placeholder, textarea:-moz-placeholder {
　color:#999 !important;
}
input::-moz-placeholder, textarea::-moz-placeholder {
　color:#999 !important;
}
input:-ms-input-placeholder, textarea:-ms-input-placeholder {
　color:#999 !important;
}

/*滚动条样式*/

 ::-webkit-scrollbar {
	/*滚动条整体样式*/
	width: 6px;
	/*高宽分别对应横竖滚动条的尺寸*/
	height: 6px;
}

 ::-webkit-scrollbar-thumb {
	/*滚动条里面小方块*/
	background: #97C7ED;
}

 ::-webkit-scrollbar-track {
	/*滚动条里面轨道*/
	background: #c8c8c8;
}

.tl{
	text-align: left !important;	
}
.tr{
	text-align: right !important;
}
.tc{
	text-align: center !important;
}
.fl{
	float: left !important;
}
.fr{
	float: right !important;
}
.cb{
	clear: both !important;
}
.dn{
	display: none;
}
.db{
	display: block;
}

/***********************************************基础元素(B)样式**************************************************/
/**
 * 
 * (B01)输入文本框
 * 
 */
.yt-input {
	width: 195px;
	height: 28px;
	line-height: 28px;
	padding-left: 4px;
	background: #FFFFFF;
	color: #333333;
	font-size: 14px;
	outline: none;
	border: 1px solid #DFE6F3;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
}
/*
 *必填字段红星标记 
 *
 */
.yt-red-star {
	color: red;
	margin-right: 2px;
}
/*
 * 输入框鼠标悬浮效果
 */
.yt-input:hover {
	border-color: #bcc8dd!important;
}
.yt-input:focus {
	border-color: #4387cd!important;
}
/**
 *
 * (B02)下拉列表
 *
 **/
.nice-select {
	-webkit-tap-highlight-color: transparent;
	background-color: #FFFFFF;
	border: solid 1px #dfe6f3;
	box-sizing: border-box;
	clear: both;
	cursor: pointer;
	display: block;
	float: left;
	font-family: inherit;
	font-size: 14px;
	color: #333333;
	font-weight: normal;
	height: 30px;
	line-height: 28px;
	outline: none;
	padding-left: 4px;
	padding-right: 23px;
	position: relative;
	text-align: left !important;
	/*-webkit-transition: all 0.2s ease-in-out;
	transition: all 0.2s ease-in-out;
	-webkit-user-select: none;*/
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	white-space: nowrap;
	width: auto;
	color: #333;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
}
/**
 * 
 *输入框中显示的文本
 *
 */
.nice-select .current {
	width: 100%;
	color: #999;
	display: inline-block;
	overflow: hidden;
}
/**
 * 
 *下拉列表输入框鼠标悬浮效果
 *
 */
.nice-select:hover {
	border-color: #bcc8dd!important;
}
/**
 * 
 *输入框中方向标
 *
 */
.nice-select:after {
	border-bottom: 2px solid #363845;
	border-right: 2px solid #363845;
	content: '';
	display: block;
	height: 5px;
	margin-top: -4px;
	pointer-events: none;
	position: absolute;
	right: 12px;
	top: 50%;
	-webkit-transform-origin: 66% 66%;
	-ms-transform-origin: 66% 66%;
	transform-origin: 66% 66%;
	-webkit-transform: rotate(45deg);
	-ms-transform: rotate(45deg);
	/*transform: rotate(45deg);
	-webkit-transition: all 0.15s ease-in-out;
	transition: all 0.15s ease-in-out;*/
	width: 5px;
}
/**
 * 
 *下拉列表展开后方向标动画
 *
 */
.nice-select.open:after {
	-webkit-transform: rotate(-135deg);
	-ms-transform: rotate(-135deg);
	transform: rotate(-135deg);
}
/**
 * 
 *下拉列表展开动画
 *
 */
.nice-select.open .list {
	opacity: 1;
	pointer-events: auto;
	-webkit-transform: scale(1) translateY(0);
	-ms-transform: scale(1) translateY(0);
	transform: scale(1) translateY(0);
}
/**
 * 
 *下拉列表禁用样式
 *
 */
.nice-select.disabled {
	border-color: #ededed;
	color: #999;
	pointer-events: none;
}
/**
 * 
 *禁用后添加边框颜色
 *
 */
.nice-select.disabled:after {
	border-color: #cccccc;
}

.nice-select.wide {
	width: 100%;
}

.nice-select.wide .list {
	left: 0 !important;
	right: 0 !important;
}

.nice-select.right {
	float: right;
}

.nice-select.right .list {
	left: auto;
	right: 0;
}

.nice-select.small {
	font-size: 12px;
	height: 36px;
	line-height: 34px;
}

.nice-select.small:after {
	height: 4px;
	width: 4px;
}

.nice-select.small .option {
	line-height: 34px;
	min-height: 34px;
}
/**
 * 
 *插件生成div列表样式
 *
 */
.nice-select .list {
	width: 100%;
	background-color: #fff;
	border: 1px solid #e5e5e5;
	margin-left: -1px;
	max-height: 200px;
	overflow: auto;
	opacity: 0;
	overflow: hidden;
	padding: 0;
	border-radius: 2px;
	box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.3);
	pointer-events: none;
	position: absolute;
	top: 100%;
	left: 0;
	-webkit-transform-origin: 50% 0;
	-ms-transform-origin: 50% 0;
	transform-origin: 50% 0;
	-webkit-transform: scale(0.75) translateY(-21px);
	-ms-transform: scale(0.75) translateY(-21px);
	transform: scale(0.75) translateY(-21px);
	-webkit-transition: all 0.2s cubic-bezier(0.5, 0, 0, 1.25), opacity 0.15s ease-out;
	transition: all 0.2s cubic-bezier(0.5, 0, 0, 1.25), opacity 0.15s ease-out;
	z-index: 1111;
	max-height: 200px;
	overflow: auto;
}

.nice-select ul.list{
	 top: inherit;
    bottom: 27px;
}
/**
 * 
 *插件生成div列表下数据样式
 *
 */
.nice-select .option {
	cursor: pointer;
	width: auto;
	color: #333;
	font-weight: 400;
	line-height: 25px;
	list-style: none;
	min-height: 25px;
	outline: none;
	padding-left: 4px;
	padding-right: 29px;
	text-align: left;
	-webkit-transition: all 0.2s;
	transition: all 0.2s;
}
/**
 * 
 *下拉列表选中样式
 *
 */
.nice-select .option.focus,
.nice-select .option.selected.focus {
	color: #333333;
	background-color: #FAFAFA;
	border: 1px dotted #DFE6F3;
}
/**
 * 
 *下拉列表悬浮样式
 *
 */
.nice-select .option:hover {
	background-color: #F6F6F6;
}
.nice-select .option.disabled {
	background-color: transparent;
	color: #999;
	cursor: default;
}
/**
 * 
 *下拉列表支持可搜索数据框样式
 *
 */
.nice-select .search-current {
	padding-left: 0px;
	width: 100%;
	border: none;
	outline: none;
	font-size: 14px;
	color: #999;
	background: none;
}

.no-csspointerevents .nice-select .list {
	display: none;
}

.no-csspointerevents .nice-select.open .list {
	display: block;
}

/**
 * 
 *select 宽度设置
 *
 */
.yt-select {
	padding-left: 4px;
	border: solid 1px #dfe6f3;
	height: 30px;
    line-height: 28px;
    color: #999 !important;
    outline: none;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
}
/**
 * 
 *(B03)按钮
 *
 */
.yt-option-btn{
	padding:0px 10px 1px 10px;
	height: 30px;
	color:#FFFFFF;
	text-align: center;
	vertical-align: middle;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	cursor: pointer;
	outline: none;
    background-color: #de595a;
    border-radius: 3px;
    border: none;
}
/**
 *查询按钮样式
 */
.yt-search-btn,.yt-common-btn{
	background: #417095;
	border: 1px solid #417095;
	color:#FFFFFF;
}
/**
 * 重置按钮样式
 */
.yt-reset-btn,.yt-cancel-btn{
	background: #d6e6f3;
	border: 1px solid #d6e6f3;
	color: #666;
}

/**
 * 
 *按钮鼠标悬浮效果
 *
 */
.yt-option-btn:hover {
	opacity: 0.8;
	filter: alpha(opacity=80);
}

/**
 * 
 *按钮图片边距
 *
 */
.yt-option-btn img {
    vertical-align: middle;
 	margin-right: 3px;
    margin-bottom: 2px;
}
/**
 * 
 *点击后的按钮
 *
 */
.yt-option-btn.yt-btn-sure {
	background: #3c7ab9 !important;
}
/**
 * 
 *禁用的按钮样式
 *
 */
.yt-option-btn.yt-btn-disabled {
	background: #FAFAFA !important;
	color: #E5E5E5 !important;
	border: 1px solid #FAFAFA;
	cursor: default;
}
/**
 * 
 *(B04)表格
 *
 */

.yt-table {
	border-collapse: collapse;
	table-layout: fixed;
}
/**
 * 
 *表头样式
 *
 */
.yt-thead th {
	padding: 4px;
	vertical-align: middle;
	border-top: none !important;
	height: 22px;
	background: #475c6d;
	color: #fff;
	font-size: 14px;
	font-weight: normal;
	border: 1px solid #D6E6F3;
}
/**
 * 
 *表格行样式
 *
 */
.yt-tbody tr {
	border: 1px solid #EDEBEC;
	cursor: pointer;
}
/**
 * 
 *表格列样式
 *
 */
.yt-tbody td {
	padding: 6px 4px;
	vertical-align: middle;
	font-size: 14px;
	text-align: left;
}

/**
 *表格文本过长，换行左对齐 
 *
 */
.yt-table-br {
	text-align: left;
}

/**
 * 
 * 表格行数据悬浮样式
 * 
 */
.yt-table tbody tr:hover {
	background: #fafafa;
}
/**
 * 
 *选中的行样式 
 *
 */
.yt-table-active {
	background: #EDEBEC !important;
}

/**
 *
 *(B05)单选按钮
 *
 *
 */

label.yt-checkbox,
label.yt-radio {
	cursor: pointer;
	padding-left: 25px !important;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	height: 20px;
    display: inline-block;
}

/*label {
	display: inline-block;
	height: 22px;
}*/
/**
 * 
 * radio默认背景
 *
 */
label.yt-radio {
	background: url(../../images/icons/lsqwhite-radio.png) 0 -40px no-repeat !important;
}
/**
 * 
 * radio禁用背景
 *
 */
label.yt-radio.yt-radio-disabled {
	background: url(../../images/icons/lsqwhite-radio.png) 0 -100px no-repeat !important;
}

/**
 * 
 * 全选;多选默认样式
 * 
 */
label.yt-checkbox input,
label.yt-radio input {
	position: absolute;
    left: 0 !important;
    cursor: pointer;
    opacity: 0 !important;
    filter:Alpha(opacity=0);
    width: auto;
}
/**
 * 
 * radio默认选中背景
 *
 */
label.yt-radio.check {
	background: url(../../images/icons/lsqwhite-radio.png) 0 0px no-repeat !important;
	position: relative;
}

/**
 * 
 * 
 * (B06)多选按钮
 *
 * 
 */

/**
 * 
 * checkbox默认背景
 *
 */
label.yt-checkbox {
	background: url(../../images/icons/white_check.png) 0 -31px no-repeat !important;
}
/**
 * 
 * checkbox默认选中背景
 *
 */
label.yt-checkbox.check {
	background: url(../../images/icons/white_check.png) 0 3px no-repeat !important;
	position: relative;
}
/**
 * 
 * checkbox禁用背景
 *
 */
label.yt-check-disabled {
	background: url(../../images/icons/white_check.png) 0 -65px no-repeat !important;
}
/**
 * 
 * 
 * checkbox半选背景
 *
 *
 */
label.yt-checkbox-half {
	background: url(../../images/icons/white_check.png) 0 -99px no-repeat !important;
}

/**
 * 
 * (B07)滚动条
 *
 */
.yt-scroll-model {
	width: 400px;
	height: 300px;
	padding: 10px;
	overflow: auto;
}
/*
 *
 *(B08)进度条
 *
 */
.yt-progress{
	padding: 6px;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: #f0f0f0;
	width: 260px;
	height: 12px;
	margin-bottom: 50px;
}
.yt-progress-probar{
	float:left;
	width:260px;
	background:#d8d8d8;
	height:12px;
	display:inline-block;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
	display:none;
	position:relative;
	left:0px;
	top:0px;
}
/**
 * 
 * 进度条定位
 * 
 */
#yt-progress-line{
   	position: absolute;
   	z-index: 10px;
}
/**
 * 
 * 进度条,白色原点
 * 
 */
.yt-progress-ball-div{
   	background:#fff;
   	width:14px;
   	height: 14px;
   	-webkit-border-radius: 50px;
   	-moz-border-radius: 50px;
   	border-radius: 50px;
   	position: absolute;
   	z-index:20px;
   	margin-top: -1px;
}
/**
 * 
 * 移动的标签进度条百分比显示
 * 
 */
.yt-progress-percent{
	position:absolute;
	left:0px;
	top:-32px;
	display:inline-block;
	color:#fff;
	font-size:16px;
	background: url(../images/icons/bubble.png);
	width: 50px;
	height: 28px;
	text-align: center;
}
/**
 * 
 * 进度条背景渐变
 * 
 */
.yt-progress-line{
	float:left;
	height:12px;
	overflow:hidden;
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
	background: -webkit-gradient(linear, 0 0, 0 100%, from(#b8d9fb), to(#4387cd));   
	background: -webkit-linear-gradient(left, #b8d9fb, #4387cd);   
    background: -moz-linear-gradient(left, #b8d9fb, #4387cd);   
    background: -o-linear-gradient(left, #b8d9fb, #4387cd);   
    background: -ms-linear-gradient(left, #b8d9fb, #4387cd);   
    background: linear-gradient(left, #b8d9fb, #4387cd);   
    filter: progid:DXImageTransform.Microsoft.gradient(GradientType = 1, startColorstr = #b8d9fb, endColorstr = #4387cd);  
}

/*
 *
 *(B09)文本域
 *
 *
 */
.yt-textarea {
	width: 500px;
	height: 80px;
	border: 1px solid #DFE6F3;
	font-size: 14px;
	color: #333333;
	outline: none;
	background: #FFFFFF;
	padding-left: 4px;
	resize: none;
}
/*
 *
 * 
 *文本域悬浮样式
 *
 * 
 */
.yt-textarea:hover {
	border-color: #bcc8dd!important;
}
.yt-textarea:focus {
	border-color: #4387cd!important;
}
/*
 *
 *(B10)数字输入框
 *
 *
 */
/*外层div*/
.yt-numberInput-box{
	width:200px;
	position: relative;
}
/*输入框样式*/
.yt-numberInput{
	color: #999;
    width: 174px;
    height: 28px;
    line-height: 28px;
    padding: 0 20px 0 4px;
    margin: 0;
    border-radius: 2px;
    outline: none;
    border: 1px solid #DFE6F3;
}
.yt-numberInput::-ms-clear{display: none;}
/*切换键外层样式*/
.yt-spin-wrap{
    position: absolute;
    width: 20px;
    height: 24px;
    top: 4px;
    right: 0px;
}
/*向上*/
.yt-spin-up{
    background: url(../images/icons/noUp.png) no-repeat 5px 4px;
    height: 12px;
}
/*向下*/
.yt-spin-down{
    background: url(../images/icons/noDown.png) no-repeat 5px 4px;
    height: 12px;
}
/*向上按钮鼠标悬浮样式*/
.yt-spin-up:hover{
	background:url(../images/icons/clickUp.png) no-repeat 5px 4px;
    height: 12px;
}
/*向下按钮鼠标悬浮样式*/
.yt-spin-down:hover{
	background:url(../images/icons/clickDown.png) no-repeat 5px 4px;
    height: 12px;
}
/*按钮鼠标悬浮样式*/
.yt-spin:hover{
    cursor:pointer;
}
/*输入框鼠标悬浮样式*/
.yt-numberInput:hover {
	border-color: #bcc8dd!important;
}
/*输入框获取焦点样式*/
.yt-numberInput:focus {
	border-color: #4387CD!important;
}

/**
 * 
 *B(011)超链接
 *
 */

/*超链接默认样式*/
.yt-link {
	color: #417095;
	cursor: pointer;
}
/*超链接鼠标悬浮样式*/
.yt-link:hover{
	color: #417095;
}
/*超链接鼠标点击样式*/
.yt-link:active{
	color: #3C7AB9 !important;
}
/**
 * 
 * 
 *B(13)loading
 * 
 * 
 */
.page-loading{
	width: 130px;
	height: 130px;
	position: absolute;
	top:0;
	left: 0;
	right: 0;
	bottom: 0;
	margin:auto;
}
/********************************************************控件(C)样式******************************************************/
/*
 *
 *C(03) 横向Tab 
 * 
 * 
 **/
/*横向tab样式*/
.yt-tab-row{
	width: 606px;
	border: 1px solid #DFE6F3;
	border-right: none;
	border-bottom: none;
	overflow: hidden;
}
/*横向tab列表的样式*/
.yt-tab-row-list{
	float: left;
	width: 150px;
	height: 35px;
	line-height: 35px;
	text-align: center;
	border-right: 1px solid #DFE6F3;
	border-bottom: none;
	cursor: pointer;
	color: #333333;
}
/*横向tab内容的样式*/
.yt-tab-row-content{
	width: 603px;
	height: 100px;
	border: 1px solid #DFE6F3;
	border-top: none;
	border-left:none;
}
/*横向tab列表鼠标悬浮的样式*/
.yt-tab-row-list:hover{
	background: #699fd7 !important;
	color: #FFFFFF;
}
/*点击后添加的样式*/
.yt-tab-active{
	background: #4387CD !important;
	color: #FFFFFF !important;
}

/**
 * 
 * C(07)消息气泡
 * 
 * 
 * */
.yt-bubble{
	background-color: #fd4040;
	width: 25px;
	height: 25px;
	font-size: 18px;
	text-align: center;
	border-radius: 12px;
	-webkit-border-radius: 12px;
	-moz-border-radius: 12px;
	color: #fff;
	display: inline-block !important;
	cursor: pointer;
}
/**
 * 气泡悬浮样式
 */
.yt-bubble:hover{
	background-color: rgb(253,102,102) !important;
}
/**
 * 气泡点击样式
 */
.yt-bubble:active{
	background-color: #fd4040 !important;
}
/**
 * 气泡超过3位数,加上右上角文字标签
 */
.yt-bubble sup{
	font-size: 10px;
    position: absolute;
}

/*
 * 
 *C(10)列表组 
 * 
 **/
.yt-list-ul{
    width:85%;
    height: auto;
    border: 1px solid #DFE6F3;
    border-bottom: none;
    background: #FFFFFF;
    font-size: 14px;
    color: #333333;
}
.yt-list-li{
    list-style: none;
    border-bottom: 1px solid #DFE6F3;
