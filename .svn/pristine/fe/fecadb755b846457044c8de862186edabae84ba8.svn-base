/**
 * 
 * 弹出框C19样式
 * 
 */
#pop-modle-alert,#pop-modle-bg {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	z-index: 100;
	background-color: #000000;
	opacity: 0.8;
	filter: alpha(opacity=80);
	display: none;
}
#pop-modle-bg {
	z-index:1000;
}
.yt-pop-model {
	height: auto;
	left: 0;
	/*right: 0;*/
	margin: auto;
	display: none;
	z-index: 103;
	background: #FFFFFF;
}
/**
 * 
 *弹出框默认表格外层 
 * 
 */
.yt-alert-table {
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	width: 100%;
	background: none;
}
.yt-alert-model{
	position: fixed;
	left: 0;
	top: 0;
	/*bottom: 0;*/
	margin: auto;
}
/**
 * 
 * 弹出框一外层
 * 
 */
.yt-alert-one {
	padding: 20px 0px;
	width: 350px;
	text-align: center;
	margin: auto;
	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
	border-radius: 2px;
	background: #fff;
}
/**
 * 
 * 弹出框一图标区域
 * 
 */
.yt-alert-one .yt-alert-one-icon {
	padding-bottom: 15px;
}
/**
 * 
 * 弹出框一图标
 * 
 */
.yt-alert-one .yt-alert-one-icon img {
	vertical-align: middle;
}
/**
 * 
 * 弹出框一提示文本区域
 * 
 */
.yt-alert-one .yt-alert-one-msg {
	font-size: 18px;
	color: #333;
	padding: 0px 20px 15px 20px;
}
/**
 * 
 * 弹出框底部按钮公用样式
 * 
 */
.yt-model-bot-btn {
	padding: 0px 20px;
	height: 30px;
	padding-bottom: 2px;
	font-size: 16px;
	-moz-border-radius: 2px;
	/* Firefox */
	-webkit-border-radius: 2px;
	/* Safari and Chrome */
	border-radius: 2px;
}
.yt-model-bot-btn:hover{
	opacity: 0.8;
	filter: alpha(opacity=80);
}
.yt-model-bot-btn.yt-model-sure-btn:active{
	background: ##=de595a !important;
	border: 1px solid #de595a;
}
/**
 * 
 * 弹出框底部确认按钮公用样式
 * 
 */
.yt-model-sure-btn {
	background: #de595a;
	border: 1px solid #de595a;
	color: #FFFFFF;
	cursor: pointer;
	outline: none;
}
/**
 * 
 * 弹出框底部取消按钮公用样式
 * 
 */
.yt-model-canel-btn {
	background: #FFF;
	border: 1px solid #cbcbcb;
	color: #666;
	margin-left: 40px;
	cursor: pointer;
	outline: none;
}
/**
 * 
 * 弹出框二外层结构样式
 * 
 */
.yt-alert-two {
	width: 150px;
	height: 150px;
	background: #FFFFFF;
	text-align: center;
	margin: auto;
	-moz-border-radius: 2px;
	/* Firefox */
	-webkit-border-radius: 2px;
	/* Safari and Chrome */
	border-radius: 2px;
}
/**
 * 
 * 弹出框二外层结构样式
 * 
 */
.yt-alert-two-icon {
	padding: 20px 0px 3px 0px;
}
/**
 * 
 * 弹出框二文本信息区域
 * 
 */
.yt-alert-two-msg {
	font-size: 18px;
	color: #333;
	padding-bottom: 3px;
}
/**
 * 
 * 弹出框二第二个文本信息区域
 * 
 */
.yt-alert-two-attach {
	font-size: 14px;
	color: #999999;
	height: 25px;
	line-height: 25px;
}
/**
 * 
 * 弹出框二倒计时样式
 * 
 */
.yt-alert-two-attach .yt-secs-num {
	font-size: 18px;
	color: #41cc6c;
	padding-left: 5px;
}
/**
 * 
 * 弹出框二关闭按钮区域
 * 
 */
.yt-alert-close {
	display: block;
	top: 10px;
	right: 10px;
	text-align: right;
	padding-right: 20px;
}
/**
 * 
 * 弹出框二关闭按钮图标
 * 
 */
.yt-alert-close img {
	cursor: pointer;
}
/**
 * 
 * 
 * 弹出框三外层结构样式
 * 
 * 
 */
.yt-edit-alert{
	position: fixed;
	background: #FFFFFF;
	border-bottom-left-radius: 3px;
	border-bottom-right-radius: 3px;
	width: 400px;
}
/**
 * 
 * 弹出框三标题区域
 * 
 */
.yt-edit-alert-title {
	background: #363845;
	height: 40px;
	line-height: 40px;
	color: #FFFFFF;
	font-size: 16px;
	-moz-user-select: none;
	-webkit-user-select: none;
	cursor: move;
	text-align: left;
	padding-left: 10px;
}
/**
 * 
 * 弹出框三标题区域图标
 * 
 */
.yt-edit-alert-title img {
	vertical-align: middle;
	margin-right: 5px;
	margin-bottom: 2px;
}
/**
 * 
 * 编辑弹出框底部样式
 * 
 */
 .yt-edit-alert .yt-eidt-model-bottom{
	text-align: center;
	height: 49px;
	line-height: 49px;
	border-top: 1px solid #dee8f4;
}
/**
 * 
 * 弹出框四外层结构
 * 
 */
.yt-alert-four {
	width: 320px;
	height: 60px;
	line-height: 60px;
	background: #000000;
	opacity: 0.6;
	filter: alpha(opacity=60);
	color: #FFFFFF;
	font-size: 18px;
	position: fixed;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
	text-align: center;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	padding: 10px;
	z-index: 111111;
}
/**
 * 
 * 
 * C(22)字段验证
 * 
 * 
 */
.valid-font {
	font-size: 12px;
	color: #f00000;
	position: absolute;
	left: 0;
	top: 30px;
	margin-left: 5px;
	margin-top: 2px;
}
/**
 * 验证标红框样式
 */
.valid-bor-sty{
	border: 1px solid #f00000 !important;
}

/**
 * 
 * 
 * C(27)加载更多
 * 
 * 
 * 
 */
.yt-load-more-model{
	padding: 30px 0px 10px 0px;
	text-align: center;
}
/**
 * 
 * 加载按钮样式
 */
.yt-load-more-model .yt-load-more-btn{
	background-color: #FFFFFF;
	border: 0px;
	width: 240px;
	height: 45px;
	color: #4387CD;
	font-size: 18px;
	cursor: pointer;
	outline: none;
	margin: 0 auto;
}
/**
 * 加载按钮中图标的样式
 */
.yt-load-more-model .yt-load-more-btn img{
	vertical-align: middle;
	margin-right: 10px;
	margin-bottom: 3px;
}
/**
 * 加载更多没有数据样式
 */
.yt-load-more-model .yt-load-no-data{
	color: #999;
	font-size: 18px;
	display: none;
}
/**
 * 
 * 加载更多loading样式
 * 
 */
.yt-more-loading-icon{
	color: #999;
	font-size: 18px;
	text-align: center;
	display: none;
}
.yt-more-loading-icon img{
	vertical-align:middle;
	margin-bottom: 3px;
    margin-right: 5px;
}
/**
 * 
 * 
 * 
 * C(28)表头筛选
 * 
 * 
 */
/**
 * 支持表头筛选的thead公用样式
 */
.yt-screen-thead thead th{
	cursor: pointer;
}
/**
 * 表头图标样式
 */
.yt-screen-thead thead th img{
	vertical-align: middle;
	margin-left: 3px;
	margin-bottom: 2px;
}
/**
 * 选中表头改变字体样式
 */
.th-font-change{
	font-style: oblique;
	font-weight: bold;
}
/**
 * 查询条件悬浮框公用样式
 */
.yt-suspension-model{
	display: none;
	font-size: 14px;
	color: #333;
	left: 0;
	text-align: left;
	position: absolute;
	background: #FFFFFF;
    border: 1px solid #999999;
    max-height: 200px;
    overflow-y: auto;
    top: 30px;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
	-moz-box-shadow: -1px 1px 16px #5c4a5c;
	-webkit-box-shadow: -1px 1px 16px #5c4a5c;
	box-shadow: -1px 1px 16px #5c4a5c;
}
.yt-suspension-model .ul-list li{
	padding: 8px;
}
.yt-suspension-model ul li{
	border: 1px solid #fff;
}
.yt-suspension-model.checobox-query{
	width: 280px;
}
.yt-suspension-model.checobox-query label.yt-checkbox{
	display: block;
    width: 100%;
    padding-left: 0px !important;
    text-indent: 30px;
    height: 24px;
    padding: 8px 0px 4px 29px;
    background: url(../images/icons/white_check.png) 10px -23px no-repeat !important;
}
.yt-suspension-model.checobox-query label.yt-checkbox.check{
	   background: url(../images/icons/white_check.png) 10px 11px no-repeat !important;
}

/**
 * 查询div里li标签选中样式
 */
.yt-suspension-model ul li.li-check{
	background-color: #e1ecf6;
	border: 1px dashed #DDDDDD;
	border-left:0px;
	border-right: 0px;
}
/**
 * 清除查询样式
 */
.yt-clear-query {
	padding: 10px;
	display: none;
}
.yt-clear-query a{
  font-size: 14px;
  color: #418dcc;
}
/*
 * 单选查询样式
 */
.radio-query{
	width: 120px;
}
.radio-query li label{
	width: 75px;
}
/**
 * 日期查询
 */
.yt-suspension-model.data-query{
	width: 100px;
	overflow-x: hidden;
}
.yt-suspension-model.data-query ul li .dadta-right-icon{
	float: right;
	padding-right: 10px;
}
.yt-suspension-model.data-query ul li .dadta-right-icon img{
	vertical-align: middle;
	margin-bottom: 3px;
}
.data-query .calendar-input{
    filter: alpha(opacity=0);
    -moz-opacity: 0;
    -khtml-opacity: 0;
    opacity: 0;
    width: 85px;
    position:absolute;
    left: 100px;
    top: -30px;
}
/**
 * 模糊查询样式
 */
.vague-query{
	width: 280px;
	padding: 10px 0px 0px 10px;
}
.vague-query ul li{
	padding: 5px 0px 5px 5px;
}
.vague-query .clear-span{
	margin-left: 4px;
}
.vague-query .clear-span a{
	color: #418dcc;
}
/**
 *数字查询样式
 */
.number-query{
	width:100px;
}
.number-query .yt-numberInput-box{
	width: 100px;
}
.number-query .yt-numberInput{
	text-align: center;
	color: #333333;
	padding: 0px;
	border: 0px;
}
.number-query .yt-numberInput:hover{
	background: none;
}
.number-query .yt-numberInput:focus{
	background-color: #e1ecf6;
}
/**
 * 
 * C(29)表格内容编辑
 * 
 */
/**
 * 表格编辑,操作图标样式
 */
.heard-handel-model{
	text-align: right;
	padding-bottom: 5px;
}
.heard-handel-model .left-handle, .heard-handel-model .right-handle{
    display: inline-block;
    cursor: pointer;
}
.heard-handel-model img {
    vertical-align: middle;
    margin-bottom: 3px;
}
.heard-handel-model span {
	color: #418dcc;
}
.heard-handel-model .right-handle {
    margin-left: 25px;
    display: none;
}
/**
 * 
 * 分页中下拉列表显示位置样式
 * 
 */
.page-select-ul-posi{
 top: 25px !important;
 bottom: inherit !important;
}

/**
 * 
 * 验证红色边框
 * 
 */
.valid-hint{
	border-color: #FF0000  !important;
}

