.border-table{
	width: 100%;
}
.border-table td,.border-table th{
	border: 1px solid #e2e9ef;
	padding: 5px;
	text-align: left;
	color: #333333;
	position: relative;
}
.border-table th{
	color: #888888;
}
.sun-head{
	background: url(../../../images/img/sun-icon.png) no-repeat;
}
.sat-head{
	background: url(../../../images/img/sat-icon.png) no-repeat;
}
.border-table th.state-day{
	text-align: center;
	width: 1px;
	padding: 0;
}
.border-table th.state-day div{
	width: 28px;
}
.state-day.all-use:after,.state-day.sub-use:after{
    content: '';
    position: absolute;
    display: inline-block;
    top: 50%;
    left: -1px;
    width: 100%;
    height: 10px;
    padding: 0 1px;
    margin-top: -5px;
    background: #df94e0;
}
.state-day.all-use:after{
	background: #847ccf;
}
.state-day.all-use.start-state:after,.state-day.sub-use.start-state:after{
	-webkit-border-radius: 5px 0 0 5px;
	-moz-border-radius: 5px 0 0 5px;
	border-radius: 5px 0 0 5px;	
}
.state-day.all-use.end-state:after,.state-day.sub-use.end-state:after{
	-webkit-border-radius: 0 5px 5px 0;
	-moz-border-radius: 0 5px 5px 0;
	border-radius: 0 5px 5px 0;
}
.state-day.all-use.end-state.start-state:after,.state-day.sub-use.end-state.start-state:after{
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.tip-table td.tip-lable {
    text-align: right;
    color: #968deb;
}
.tip-table td{
	color: #fff;
	padding: 5px 0;
}
.calendar .calMain .calContent .enabled{
	height: 80px;
}
.mouth-check{
	color: #FFFFFF;
	cursor: pointer;
}
.tip-box{
	border-top: 1px dashed #ffffff;
    color: #fff;
    padding: 0 12px;
    position: relative;
}
.tip-box:first-child{
	border: none;
}
.tip-box table td.tip-lable{
	color: #FFF;
}
.tip-box:before{
    content: '';
    display: inline-block;
    position: absolute;
    height: 12px;
    width: 5px;
    background: #968deb;
    top: 9px;
    left: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.content-box{
	background: #FFFFFF;
}
.change-week span{
	color: #7b74c7;
	font-size: 16px;
	margin: 0 15px;
	cursor: pointer;
}
.example span{
	position: relative;
	color: #555555;
	font-size: 12px;
	margin: 0 35px;
}
.example span:after{
	position: absolute;
    left: -14px;
	top: 5px;
	width: 8px;
	height: 8px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	content: '';
	display: inline-block;
	background: #b8b3e2;
}
.example span:first-child:after{
	background: #847ccf;
}
.example span:last-child:after{
	background: #df94e0;
}
.programme .list-title{
	height: 56px;
	line-height: 56px;
}
.border-table {
	width: 100%;
}

.border-table td,
.border-table th {
	border: 1px solid #e2e9ef;
	padding: 5px;
	text-align: left;
	color: #333333;
	position: relative;
}

.programme th {
	position: relative;
	color: #888888;
	padding:8px 5px;
	
}
.programme thead tr th span{
	position: absolute;
	top: 0;
	right: 0;
	text-align: right;
	font-size: 12px;
	display: inline-block;
	padding: 2px 2px 0 0;
	width: 58px;
	height: 33px;
	background: url(../../../../resources/images/img/day-head.png) no-repeat;
	color: #4c4c4c;
}
.border-table .have-couse{
 	color: #fff;
 	text-align: center;
}
.programme th.state-day div{
	width: 115px;
}
.morning .have-couse{
	background: #847ccf;
	position: relative;
	overflow: hidden;
}
.afternoon .have-couse{
	background: #b8b3e2;
}
.night .have-couse{
	background: #df94e0;
}
.programme th.state-day{
	text-align: left;
	text-indent: 15px;
}
.programme .tool-box{
	position: absolute;
	width: 101%;
	top: 0;
	left: 0;
	height: 100%;
}
.programme .tool-info{
	width: 115px;
	height: 30px;
	float: left;
}
.content-box.programme{
	margin: 0 5px;
	padding: 0 6px;
}
