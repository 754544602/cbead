.every-part {
	position: relative;
	border-radius: 10px;
	padding: 10px;
}

.every-part-1 label {
	position: absolute;
	z-index: 100;
	right: 109px;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
}

.every-part-1 span {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #de595a;
}
.every-part .average{
	position: absolute;
	top: 20px;
	right: 40px;
	z-index: 100;
}

.every-part .userCount{
	float: right;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
	margin-left: 28px;
}

.every-part .userCount-label{
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 0px;
	color: #de595a;
}
.every-part-1 .lab {
	padding: 15px 30px 7px 10px;
	box-sizing: border-box;
	background-color: #ffffff;
	box-shadow: 0px 5px 5px 0px rgba(0, 0, 0, 0.3);
	border-radius: 5px;
	border: solid 1px #eeeeee;
	position: absolute;
	right: 127px;
	top: 132px;
}

.every-part-1 div>ul>li {
	list-style: none;
	margin-bottom: 28px;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #808080;
}

.every-part-1 div>ul>li span {
	font-family: MicrosoftYaHei;
	font-size: 16px;
	letter-spacing: 0px;
	color: #666666;
	opacity: 0.8;
}


