/********** 提醒设置 Start *************/

.setting-alert {
	position: fixed;
	top: 0;
	left: 0;
	z-index: 10001;
	width: 600px;
	display: none;
}

.setting-alert .model-title {
	background-color: #417095;
	color: #fff;
	height: 40px;
	line-height: 40px;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	padding: 0 10px;
	font-size: 16px;
}

.setting-alert .model-title img {
	vertical-align: middle;
	margin-right: 10px;
	margin-bottom: 3px;
}

.setting-alert .model-content {
	background: #FFFFFF;
	padding: 0 10px;
}

.setting-alert.heard-search-model {
	padding: 20px 10px;
}

.setting-alert .model-bottom-btn {
	text-align: center;
	padding: 10px 0;
}

.setting-alert .radio-sty {
	margin-right: 0;
}

.setting-alert .setting-tab {
	padding: 10px;
}

.setting-alert .setting-tab li {
	float: left;
	padding: 0 10px;
	cursor: pointer;
}

.setting-alert .setting-tab li.check {
	color: #179ee8;
	border-bottom: 3px solid #179ee8;
	background: #FFFFFF;
	box-sizing: border-box;
}

.clearfloat:after {
	display: block;
	clear: both;
	content: "";
	visibility: hidden;
	height: 0;
}

.clearfloat {
	zoom: 1;
}

.letter-four {
	letter-spacing: 5px;
}

.setting-alert .tab-div {
	padding: 0 10px;
}

.setting-alert .form-table {
	width: 100%;
}

.setting-alert .form-table tr td {
	padding: 10px 0;
}

.setting-alert .nice-select {
	width: 300px;
	display: inline-block;
	float: none;
	vertical-align: middle;
}

.setting-alert .state-select {
	width: 150px;
}

.colon {
	margin-left: 5px;
}

.setting-alert .prev-day {
	width: 100px;
}

.predict-select {
	width: 50px !important;
}


