@charset "utf-8";
body {
	background: #FFFFFF !important;
}

#homePage {
	margin: 5px;
	min-width: 1095px;
}

#left-base {
	margin-right: 5px;
	padding: 32px 10px 0px 10px;
	background: #FFFFFF;
	width: 249px;
	height: auto;
	border: 1px solid #DFE6F3;
}

#left-base .box {
	text-align: center;
}

.users-img-bak {
	position: absolute;
	top: 47px;
	display: inline;
	height: 100px;
	width: 100px;
	/*background: url(../../images/common/users-bak-img.png) no-repeat;*/
}

.users-img-model img {
	width: 100px;
	height: 100px;
	vertical-align: middle;
}

#users-name {
	padding: 25px 0px 18px 0px;
	font-size: 24px;
	color: #222222;
}

#users-job {
	padding-bottom: 25px;
	font-size: 16px;
	color: #666666;
}

#left-ul li {
	padding: 10px 15px;
	border-top: 1px solid #EEEEEE;
}

#left-ul li img {
	vertical-align: middle;
}

#edit-phone,
#log-icon,
#update-pwd {
	float: right;
	display: inline;
	cursor: pointer;
}

.no-login {
	color: #ff3535;
}

.nav-heard {
	padding-top: 10px;
	z-index: 5;
	background: #FFFFFF;
	height: 40px;
	border-bottom: 1px solid #EEEEEE;
}

.tab-nav {
	margin-right: 30px;
	position: relative;
	float: left;
	display: inline;
	padding: 0px 10px;
	height: 40px;
	line-height: 38px;
	font-size: 16px;
	text-align: center;
	cursor: pointer;
	box-sizing: border-box;
}

.active-nav {
	color: #179ee8;
	border-bottom: 3px solid #179ee8;
	background: #FFFFFF;
	box-sizing: border-box;
}

#right-matter {
	padding-left: 10px;
	width: 100%;
	padding-top: 5px;
	background: #FFFFFF;
	border: 1px solid #DFE6F3;
}

.condition-model {
	padding: 10px 20px 10px 0px;
	font-size: 16px;
	text-align: right;
	border-bottom: 1px solid #EEEEEE;
}

.condition-model label {
	cursor: pointer;
}

.condition-model img {
	margin-right: 8px;
	margin-bottom: 3px;
	vertical-align: middle;
}

.condition-title {
	padding: 10px 113px 10px 10px;
	color: #222222;
	font-size: 16px;
}

.tab-model ul li {
	border-top: 1px solid #EEEEEE;
}

.backlog-model ul li {
	padding: 10px 113px 10px 10px;
	font-size: 14px;
}

.caution-model ul li {
	padding: 10px 17px 10px 10px;
}

.tab-model ul li div {
	display: inline;
	color: #666666;
}

.d-right {
	float: right;
	display: inline
}

.font-col {
	color: #179ee8;
	cursor: pointer;
}

.tab-content {
	display: none;
}

.echarts-box {
	position: relative;
	/*height: 300px;*/
}

.echarts-box .tab-content {
	display: block;
	width: 100%;
	height: 100%;
	/*position: absolute;
	left: -11111px;*/
}

.war-font-co {
	color: #ff3535;
}

.look-span {
	margin-right: 25px;
}

.day-span {
	margin-right: 45px;
}

.yt-bubble {
	top: 2px;
	position: absolute;
	background-color: #ff3535;
	width: 16px;
	height: 16px;
	line-height: 16px;
	font-size: 14px;
	text-align: center;
	border-radius: 12px;
	-webkit-border-radius: 12px;
	-moz-border-radius: 12px;
	color: #fff;
	display: inline-block !important;
	cursor: pointer;
}

.radio-sty input {
	margin-right: 10px;
	position: absolute;
	left: 0 !important;
	cursor: pointer;
	-webkit-opacity: 0;
	-moz-opacity: 0;
	-khtml-opacity: 0;
	opacity: 0;
	filter: alpha(opacity=0);
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
	filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
	background: url(../../images/index/radio2.png) 2px 3px no-repeat !important;
	position: relative;
	vertical-align: middle;
}

.radio-sty {
	margin-right: 15px;
}

.radio-sty.check {
	background: url(../../images/index/radio2.png) 0px 5px no-repeat !important;
	position: relative;
}

.radio-sty {
	background: url(../../images/index/radio1.png) 0px 5px no-repeat !important;
	position: relative;
}


/**
 * 
 * 
 * 
 * 系统模块区域
