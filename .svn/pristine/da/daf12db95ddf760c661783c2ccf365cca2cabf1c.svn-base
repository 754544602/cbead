.add-title{
    background: #363845;
    color: #FFFFFF;
	font-size: 18px;
	padding-left: 27px;
	height: 40px;
	line-height: 40px;
}
.class-info {
	background-color: #ffffff;
	border-radius: 5px;
	border: solid 1px #f5f5f5;
}
.class-info-centent {
	margin-left: 20px;
	margin-top: 17px;
	height: auto;
}
.entrust-unit-centent {
	margin-top: 0px;
	height: auto;
}
.project-leader-inf {
	margin-left: 20px;
	margin-top: 0px;
	height: auto;
}
.person-info-title {
	margin: 7px 0px 0px 20px;
}
.class-info-table tr {
	height: 53px;
}
.class-info-table,.teach-plan-table {
	margin: 43px auto;
}
.add-project .yt-input,.project-details .yt-input{
	width: 280px;
}
.project-name,.customer-trainee-age,.project-head-name,.project-headmaster-name,.project-aid-name{
	width: 720px;
}
.details,.customer-target-demand{
	width: 720px;
	height: 128px;
}
.btn-all{
	width: 175px;
	margin: 20px auto;
	position: static;
	margin-bottom: 20px;
}
.btn-all>.btn-off{
	background: #FFFFFF;
	color: #000;
	border:1px solid #cbcbcb;
}
.yt-textarea{
	resize: none;
}
.add-project .nice-select{
	width: 285px;
}
.btn-return{
	margin-top: 5px;
}
.project-details .class-info-table input,.project-details .class-info-table textarea{
	border: none;
}
.class-info-div {
    position: relative;
    height: 45px;
    line-height: 45px;
    font-size: 18px;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 0px;
    color: #de595a;
    padding-left: 30px;
}
.class-info-div:before {
    position: absolute;
    top: 50%;
    margin-top: -13px;
    left: 6px;
    content: '';
    display: inline-block;
    height: 26px;
    width: 6px;
    -webkit-border-radius: 1px;
    -moz-border-radius: 1px;
    border-radius: 1px;
    background: #de595a;
}
.cont-edit-test td{
	height: 50px;
}
.alert-form{
	overflow: auto;
}
.charge-tbody td,.personal-standard td{
	height: 42px;
	border: 1px solid #e6e6e6;
	text-align: center;
}
.charge-tbody input,.personal-standard input,.student-cost-tbody input{
	width: 80%;
}
.charge-tbody input,.charge-tbody .nice-select{
	background: none;
}
.project-charge th{
	border: 1px solid #e6e6e6;
	background: #f5f5f5;
}
.list-box{
	background: #FFFFFF;
	border: none;
}
.unit-tfoot td{
	height: 28px;
	border: 1px dashed #e6e6e6;
}
.add-charge{
	color: #7b74c7;
	cursor: pointer;
}
.btn-sub-charge{
	margin-right: 20px;
}
.btn-charge{
	width: 125px;
	margin: 22px auto;
}
.student-charfe-inf-table{
	margin-top: 20px;
}
.yt-option-btn{
	margin-right: 10px;
}
.teach-plan-table td{
	height: 50px;
}
.teach-related-expenses,.logistics-related-expenses{
	margin: 0 auto;
	position: relative;
	width: 860px;
	border: 1px solid #f2f2f2;
	margin-bottom: 12px;
}
.teach-related-expenses-title>span{
	position: absolute;
	background: #847ccf;
	padding: 7px 8px 8px 7px;
	border-radius: 2px;
	color: #FFFFFF;
	font-size: 16px;
}
.teach-related-expenses input,.logistics-related-expenses input{
	width: 140px;
}
.teach-related-expenses .gist,.logistics-related-expenses .gist{
	width: 520px;
}
.teach-related-expenses span,.logistics-related-expenses span{
	margin-left: 12px;
}
.offsite-training-requirement{
	width: 715px;
	height: 130px;
}
.discuss-info-table td{
	height: 50px;
}
.valid-font{
	top: 40px;
}
.next-operator{
	margin-top: 7px;
}
.next-operator table{
	margin: 12px auto;
}
.entrust-inf .yt-input{
	width: 280px;
}
.first-draft-inf{
	width: 875px;
	margin: 30px auto;
}
.file-id>p img{
	position: relative;
	top: 2px;
	margin: 0px 12px 0px 50px;
}
.file-id>p{
	padding: 0px 18px 0px 18px;
}
.personal-standard input{
	margin-left: 3px;
	width: 80%;
}
.student-charfe-inf-table>.student-cost-tbody>tr>td{
	text-align: center;
}
.project-schedule{
	height: 100px;
	text-align: center;
	margin: 20px 0px 20px 0px;
}
.project-schedule>span{
	display: inline-block;
}
.project-schedule>.process{
	position: relative;
	bottom: 60px;
}
.chargePersonalInput{
	border: none;
	text-align: center;
	disabled:true;
}
