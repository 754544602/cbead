.educationIndex .state-day {
	width: 30px;
	height: 30px;
}

.educationIndex .state-day.all-use:after,
.educationIndex .state-day.sub-use:after,
.educationIndex .state-day.no-use:after,
.educationIndex .state-day.alr-use:after {
	content: '';
	display: inline-block;
	position: absolute;
	top: auto;
	margin: auto;
	margin-top: -11px;
	width: 100%;
	height: 22px;
	padding: 0 1px;
	
}
/*未立项*/
.state-day.sub-use:after{
	background-color: #DBDBDB;
}
/*已立项*/
.state-day.all-use:after {
	background-color: #847CCF;
}
/*确定意向*/
.state-day.sur-use:after {
	background-color: #ec577c;
}
/*已运行*/
.state-day.no-use:after {
	background-color: #DF94E0;
}
/*已结项*/
.state-day.alr-use:after {
	background-color: #F6E7CD;
}

.state-day.all-use.start-state:after,
.state-day.sub-use.start-state:after,
.state-day.no-use.start-state:after,
.state-day.sur-use.start-state:after,
.state-day.alr-use.start-state:after {
	-webkit-border-radius: 3px 0 0 3px;
	-moz-border-radius: 3px 0 0 3px;
	border-radius: 3px 0 0 3px;
}

.state-day.all-use.end-state:after,
.state-day.sub-use.end-state:after,
.state-day.no-use.end-state:after,
.state-day.sur-use.end-state:after,
.state-day.alr-use.end-state:after {
	-webkit-border-radius: 0 5px 5px 0;
	-moz-border-radius: 0 5px 5px 0;
	border-radius: 0 5px 5px 0;
}

.state-day.all-use.end-state.start-state:after,
.state-day.sub-use.end-state.start-state:after,
.state-day.no-use.end-state.start-state:after,
.state-day.sur-use.end-state.start-state:after,
.state-day.alr-use.end-state.start-state:after {
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}

