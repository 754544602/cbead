<!DOCTYPE html>
<html>

	<head>
		<meta http-equiv="x-ua-compatible" content="ie=Edge" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<!--样式文件引用-->
		<link rel="stylesheet" type="text/css" href="../../resources/js/scroll/jquery.mCustomScrollbar.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/common/yt-common.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/page/yt-page.min.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/js/jquery-easyui-1.5.1/themes/default/easyui.css" />
		<link rel="stylesheet" type="text/css" href="../../resources/css/modules/class/classInfo.css" />
		<!--日期控件-->
		<link rel="stylesheet" type="text/css" href="../../resources/css/calendar/yt-calendar.min.css" />
		<title>办班动态</title>
		<style type="text/css">
			.div-button{
				text-align:center;
				margin-top: 20px;
			}
			.div-time{
				text-align:left;
				margin-top: 20px;
			}
			.small1 { width:30%; float:left;margin-bottom: 30px;}
    		.small2 { width:70%; float:right;margin-bottom: 30px;text-align: right;}
		</style>
	</head>
	<body>
		<div id="project-main" style="min-width: 1000px;">
			<div class="page-title">
			    <div class="div-time">
					<label style="color: white;">统计时段: </label>  
					<input id="start-time" class="calendar-input" style="width: 150px;height: 30px;" value="" placeholder="开始日期" type="text" />  
					<label style="color: white;">至 </label>  
					<input id="end-time" class="calendar-input" style="width: 150px;height: 30px;" value="" placeholder="结束日期" type="text" /> 
			    </div>
				<button class="page-return-btn">
					<img class="class-return-img" src="../../resources/images/icons/class-return.png"/>
					返回
				</button>
			</div>
		    <!-- 全部图表 start -->
			<div class="summary-box">
				<!--班次统计的divSTART-->
				<div class="every-part" style="width: 100%">
					<!-- 页面满意度的 DOM -->
						<div class="drawing-1" style="width: 60%;height:600px;float: left;"></div>
						<div class="drawing-2" style="width: 39%;height:600px;float:right;"></div>
					<!--每一栏的divEND-->
				</div>
				<!-- 办班动态列表查询  start-->
				<div class="details-box">
					<div class="search-btn-slect">
						<div class="btn-all tb1" style="position: relative;width: 99%;">
							<div class="small1">
								<span style="font-size:20px;">班次列表</span>
							</div>
							<div class="small2">
								<label class="check-label yt-checkbox check">  
				                    <input id="checkBox1" type="checkbox" name="test" value="1"/>未开始
				                </label>  
				                <label class="check-label yt-checkbox check">  
				                    <input id="checkBox2" type="checkbox" name="test" value="2"/>培训中
				                </label>
				                <label class="check-label yt-checkbox check">  
				                    <input id="checkBox3" type="checkbox" name="test" value="3"/>已结束
				                </label>
								<button class="yt-option-btn exportList">
									导出
								</button>
							</div>
						</div>
					</div>
					<div class="list-box" style="border: none;">
						<table class="yt-table list-table">
							<thead class="list-thead">
								<tr>
									<th>编号</th>
									<th>班级名称</th>
									<th>类型</th>
									<th>委托单位</th>
									<th>项目主任</th>
									<th>开班日期</th>
									<th>结束日期</th>
									<th>培训天数</th>
									<th>学员数量</th>
									<th>状态</th>
								</tr>
							</thead>
							<tbody class="yt-tbody class-tbody list-tbody">
								<tr class="class-tr">
									<td colspan="9" align="center" style="border:0px;">
										<div class="no-data" style="width: 280px;margin: 0 auto;">
											<img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;">
										</div>
									</td>
								</tr>
							</tbody>
						</table>
						<div class="page-info table-page page1" onselectstart="return false"></div>
					</div>
					<!--每一栏的divEND-->
				</div>
				<!-- 办班动态列表查询  end-->
				<!--统计列表的divSTART-->
				<div class="every-part">
					<!-- 页面满意度的 DOM -->
						<div class="drawing-4" style="width: 100%;height:600px;"></div>
					<!--每一栏的divEND-->
				</div>
			
				<!-- 班次统计列表 -->
				<div class="list-box2" style="border: none;width:100%;">
				    <span style="font-size:20px;">班次统计列表</span>
					<table class="yt-table list-table">
							<thead class="list-thead">
							    <tr>
						    		<th>类型</th>
						    		<th colspan="4" align="center">计划班次</th>
						    		<th colspan="4" align="center">委托班次</th>
						    		<th colspan="4" align="center">选学班次</th>
						    		<th colspan="4" align="center">调训班次</th>
						    		<th colspan="4" align="center">小计</th>
					    		</tr>
								<tr>
									<th>月份</th>
									<th>期数</th>
									<th>天数</th>
									<th>人数</th>
									<th>人天数</th>
									<th>期数</th>
									<th>天数</th>
									<th>人数</th>
									<th>人天数</th>
									<th>期数</th>
									<th>天数</th>
									<th>人数</th>
									<th>人天数</th>
									<th>期数</th>
									<th>天数</th>
									<th>人数</th>
									<th>人天数</th>
									<th>期数</th>
									<th>天数</th>
									<th>人数</th>
									<th>人天数</th>
								</tr>
							</thead>
							<tbody class="yt-tbody class-tbody list-tbody tableList">
								<tr class="class-tr">
									<td colspan="20" align="center" style="border:0px;">
										<div class="no-data" style="width: 280px;margin: 0 auto;">
											<img src="../../resources/images/common/no-data.png" alt="" style="padding: 35px 0 20px;">
										</div>
									</td>
								</tr>
							</tbody>
					</table>
				</div>
			</div>
			<!-- 全部图表的 end -->
		</div>
	</body>
	<!--js文件引用-->
	<script type="text/javascript" src="../../resources/js/common/jquery.min.js"></script>
	<script type="text/javascript" src="../../resources/js/jquery-easyui-1.5.1/jquery.easyui.min.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-option.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-common.min.js"></script>
	<script type="text/javascript" src="../../resources/js/common/yt-frame.min.js"></script>
	<script type="text/javascript" src="../../resources/js/page/yt-page.min.js"></script>
	<script type="text/javascript" src="../../resources/js/valid/yt-valid.min.js"></script>
	<script type="text/javascript" src="../../resources/js/fileUpload/ajaxfileupload.min.js"></script>
	<!--日期控件-->
	<script type="text/javascript" src="../../resources/js/calendar/yt-calendar.min.js"></script>
	<!--雷达图js-->
	<script type="text/javascript" src="../../resources/js/echarts/echarts.min.js"></script>
	<!--滚动条js-->
	<script type="text/javascript" src="../../resources/js/scroll/jquery.mCustomScrollbar.concat.min.js"></script>
	<!-- 获取数据的js -->
	<script type="text/javascript" src="../../resources/js/modules/classdynamic/classDynamicList.js"></script>

</html>