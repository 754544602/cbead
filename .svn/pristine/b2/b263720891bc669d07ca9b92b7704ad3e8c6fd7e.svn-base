.meeting-room{
	width: 100%;
}
.meeting-room td,.meeting-room th{
	border: 1px solid #e2e9ef;
}
.meeting-room td{
	padding: 8px 5px;
	position: relative;
	cursor: pointer;
}

.meeting-room th{
	font-size: 12px;
	color: #4c4c4c;
	padding: 10px 0;
	position: relative;
	height: 20px;
}
.meeting-room th span{
	left: -18px;
    display: inline-block;
    height: 35px;
    background: #fff;
    position: absolute;
    top: 0;
    line-height: 35px;
}
.meeting-room th div{
	width: 40px;
}
div.select-item,div.old-item{
	position: absolute;
    top: 50%;
    left: -1px;
    height: 22px;
    width: 100%;
    padding: 0 1px;
    background: #847ccf;
    margin-top: -11px;
}
div.select-item{
	background: #de595a;
}
div.select-item.start,div.old-item.start{
	-webkit-border-radius: 5px 0 0 5px;
	-moz-border-radius: 5px 0 0 5px;
	border-radius: 5px 0 0 5px;	
	z-index: 1;
}
div.select-item.end,div.old-item.end{
	-webkit-border-radius: 0 5px 5px 0;
	-moz-border-radius: 0 5px 5px 0;
	border-radius: 0 5px 5px 0;
}
div.select-item.start.end,div.old-item.start.end{
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
div.ganttview-vtheader-title{
	border: none;
}
.form-label{
	text-align: right;
}
.yt-edit-alert-main td{
	padding: 10px 0;
	width: 130px;
	position: relative;
}
.time-center{
	position: relative;top: -10px;display: inline-block;width: 9px;text-align: center;
}
.tool-box{
	height: 22px;
	color: #FFF;
	text-align: center;
}
			
.tip-table td.tip-lable {
    text-align: right;
    color: #968deb;
}
.tip-table td{
	color: #fff;
	padding: 5px 0;
}
.calendar .calMain .calContent .enabled{
	height: 80px;
}
.mouth-check{
	color: #FFFFFF;
	cursor: pointer;
}
.tip-box{
	border-top: 1px dashed #ffffff;
    color: #fff;
    padding: 0 12px;
    position: relative;
}
.tip-box:first-child{
	border: none;
}
.tip-box table td.tip-lable{
	color: #FFF;
}
.tip-box:before{
    content: '';
    display: inline-block;
    position: absolute;
    height: 12px;
    width: 5px;
    background: #968deb;
    top: 9px;
    left: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.content-box{
	background: #FFFFFF;
}
.current-day{
	font-size: 24px;
	font-weight: bold;
	padding: 0 30px;
	cursor: pointer;
}
.prev-day,.next-day{
	font-size: 18px;
	color: #CCCCCC;
	cursor: pointer;
}
			
