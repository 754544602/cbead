	<body>
		<div id="reimApproList">
			<!--顶部条件查询START-->
			<div class="head-div">
				<span class="sel-text">关键字：</span>
				<input type="text" class="yt-input search" id="keywordInput" placeholder="请输入关键字" />
				<img class="clearImg" src="../../../../../resources-sasac/images/system/expensesReim/common/sc_ret.png" />
				<button class="yt-option-btn yt-search-btn" onselectstart="return false" id="heardSearchBtn">关键字查询 </button>
				<button class="yt-option-btn yt-reset-btn" id="resetBtn">重置</button>
			</div>
			<!--顶部条件查询END-->
			<!--数据展示区域START-->
			<div class="tab-div">
				<!--tab标签START-->
				<div class="tab-header">
					<ul>
						<li class="tab-li active-li" style="border-left: none;"><input type="hidden" value="WF_SUSPENDING_QUERY_SQL_PARAMS" />待处理</li>
						<li class="tab-li"><input type="hidden" value="WF_SOLVED_QUERY_SQL_PARAMS" />已处理</li>
						<li class="tab-li"><input type="hidden" value="WF_COMPLETED_QUERY_SQL_PARAMS" />已完结</li>
						<!--<li class="tab-li"><input type="hidden" value="WF_DRAFTS_QUERY_SQL_PARAMS" />草稿箱</li>-->
					</ul>
				</div>
				<!--tab标签END-->
				<!--第一页  待处理-->
				<div class="tab-content">
					<table class="yt-table pending">
						<thead class="yt-thead">
							<tr>
								<th style="width: 150px;">单据编号</th>
								<th>付款事由</th>
								<th style="width: 100px;">费用类型</th>
								<th style="width: 100px;">申请人</th>
								<th style="width: 100px;">所属部门</th>
								<th style="width: 100px;">申请付款总金额(元)</th>
								<th style="width: 100px;">申请日期</th>
								<th style="width: 100px;">流程状态</th>
								<th style="width: 100px;">操作</th>
							</tr>
						</thead>
						<tbody class="yt-tbody">
							<tr style="border:0px;background-color:#fff !important;">
								<td colspan="9" align="center" style="border:0px;">
									<div class="no-data" style="width: 280px;margin: 0 auto;">
										<img src="../../../../../resources-sasac/images/common/no-data.png" alt="" style="padding: 35px 0 20px;">
									</div>
								</td>
							</tr>
						</tbody>
					</table>
					<div class="page-info table-page page1" onselectstart="return false"></div>
				</div>
				<!--第二页  已处理,已完结-->
