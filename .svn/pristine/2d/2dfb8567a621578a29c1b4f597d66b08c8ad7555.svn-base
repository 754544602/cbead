.rule-table td.cell-check {
	border: 2px solid #417095 !important;
	background: #d9e8f5 !important;
}

.rule-table-div .yt-thead tr th.cell-check {
	border: 2px solid #418dcc !important;
	background: #418dcc !important;
}

.rule-table th.indicate {
	height: 30px;
	background: #417095;
	border: 1px solid #DFE6F3;
	font-size: 12px;
	color: #fff;
	text-align: center;
	cursor: pointer;
}

.rule-table td,
.rule-table th {
	text-align: center;
}

.rule-table .yt-tbody tr.tr-shadow {
	border: 2px solid #417095 !important;
	background: #d9e8f5 !important;
}

.rule-table .yt-tbody th.indicate {
	width: 80px;
	min-width: 80px;
	max-width: 80px;
}

.rule-table .yt-tbody tr td.td-shadow {
	border-left: 2px solid #417095 !important;
	border-right: 2px solid #417095 !important;
	background: #d9e8f5 !important;
}

.rule-table .yt-tbody tr:last-child td.td-shadow {
	border-bottom: 2px solid #417095 !important;
}

.rule-table-div {
	position: relative;
	overflow: auto;
	max-height: 400px;
}

.rule-table-div table {
	position: relative;
	width: 100%;
	table-layout: auto;
}

.rule-table-div .right-button-context {
	position: fixed;
	left: 0;
	top: 0;
	min-width: 200px;
	background: #fff;
	display: none;
	z-index: 9;
}

.rule-table-div .right-button-context ul li {
	list-style: none;
	padding: 5px 10px;
	border-top: 1px solid #000;
	cursor: pointer;
}

.rule-table-div .right-button-context ul li:first-child {
	border-top: none;
}

.rule-table-div .right-button-context ul li.dis {
	color: #999;
	cursor: default;
}

.rule-table-div .right-button-context ul li.dis:hover {
	background: #fff;
	color: #999;
	cursor: default;
}

.rule-table-div .right-button-context ul li:hover {
	background: #417095;
	color: #fff;
}

.rule-table-div .input-holder {
	position: absolute;
	left: 0;
	top: 0;
	z-index: 10;
}

.rule-table-div .hand-input {
	width: 0;
	height: 0;
	border-right: 1px solid #DFE6F3;
	resize: none;
}

.rule-table-div .yt-thead tr th.tab-indicate {
	background: none;
	border: none;
}

.rule-table .yt-tbody tr.tr-shadow th.indicate{
	border: 1px solid #DFE6F3;
}
