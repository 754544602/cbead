/*头部*/

body {
	margin: 0;
	padding: 0;
}

.index-top {
	width: 100%;
	min-width: 1130px;
	background-color: #363845;
	box-shadow: inset 0px -1px 0px 0px #5c607a;
	padding: 15px 0px;
}

.index-top-middle {
	text-align: center;
}

.index-top-model {
	width: 23%;
	min-width: 260px;
	height: 124px;
	background-color: #ffffff;
	box-shadow: 0px 3px 7px 0px rgba(147, 152, 187, 0.35);
	border-radius: 2px;
	border: solid 1px #f5f5f5;
	display: inline-block;
}

.index-top-model img {
	width: 74px;
	margin: 25px 0 0 16px;
}

.index-top-model div {
	width: 55%;
	float: right;
	text-align: right;
	padding-right: 24px;
}

.index-top-model div {
	margin-top: 33px;
}

.index-top-model h5 {
	font-family: MicrosoftYaHei;
	font-size: 18px;
	font-weight: normal;
	letter-spacing: 0px;
	color: #777777;
}

.index-top-model span {
	font-family: MicrosoftYaHei-Bold;
	font-size: 24px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
}

.index-top-model label {
	font: "微软雅黑" 18px;
	color: #777777;
}

.index-top-model:not(:first-child) {
	margin-left: 12px;
}


/**/
.index-rates-yes,.index-rates-no{
	display: none;
}
.index-rates {
	width: 100%;
	min-width: 1130px;
	border: 5px solid #f5f5f5;
	padding: 8px;
	background-color: #FFFFFF;
	box-sizing: border-box;
	border-radius: 10px;
}

.index-rates thead {
	border-top: 1px solid #E6E6E6;
}

.rates-table {
	width: 100%;
	position: relative;
}

.rates-table caption {
	height: 58px;
	text-align: left;
	padding-left: 10px;
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 1px;
	color: #111111;
	position: relative;
	padding-top: 12px;
}

.rates-table caption ul {
	margin-left: -10px;
	position: absolute;
	bottom: 0;
}

.rates-table caption li {
	width: 78px;
	height: 28px;
	text-align: center;
	float: left;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #333333;
	line-height: 28px;
	margin-right: 10px;
	cursor: pointer;
}

.rates-table caption li.active {
	border-bottom: 1px solid #3c4687;
	color: #3c4687;
}

.rates-table tr {
	height: 42px;
	border-bottom: none;
	border-left: none;
	border-right: none;
}

.rates-table th {
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
}

.rates-table td {
	font-family: MicrosoftYaHei;
	font-size: 12px;
	letter-spacing: 0px;
	color: #333333;
	padding: 0;
}

.rates-table td:nth-child(1),
.rates-table td:nth-child(2),
.rates-table th:nth-child(1),
.rates-table th:nth-child(2),
.rates-table td:nth-child(4),
.rates-table th:nth-child(4){
	text-align: left;
}
.rates-table td:nth-child(5),
.rates-table td:nth-child(6),
.rates-table th:nth-child(5),
.rates-table th:nth-child(6){
	text-align: right;
}
.rates-table td:nth-child(3),
.rates-table td:nth-child(7) {
	text-align: center;
}

.rates-table td:nth-child(1),
.rates-table th:nth-child(1) {
	padding-left: 6px;
}


/*日程表*/
.tip-table td.tip-lable {
    text-align: right;
}
.tip-table td{
	color: #fff;
	padding: 5px 0;
}
.tip-box{
	border-top: 1px dashed #ffffff;
    color: #fff;
    padding: 0 12px;
    position: relative;
}
.tip-box:first-child{
	border: none;
}
.tip-box table td.tip-lable{
	color: #FFF;
}
.tip-box:before{
    content: '';
    display: inline-block;
    position: absolute;
    height: 12px;
    width: 5px;
    background: #968deb;
    top: 9px;
    left: 0;
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
}

.calendar .calMain .calContent .enabled{
	height:auto
}
.calendar .calMain .calContent .enabled td{
	border: none;
}
.index-plan {
	width: 100%;
	min-width: 1130px;
	background-color: #FFFFFF;
	box-sizing: border-box;
	padding: 8px;
	border: 5px solid #f5f5f5;
	border-top: none;
	border-radius: 10px;
}

.plan-table {
	width: 100%;
}

.plan-table th {
	border: 1px solid #e2e9ef;
	font-family: MicrosoftYaHei;
	font-size: 14px;
	letter-spacing: 0px;
	color: #888888;
	cursor: pointer;
}
.plan-table tr{
	border: none;
}
.plan-table td {
	position: relative;
	padding: 0;
	border: 1px solid #E2E9EF;
}
.plan-table th:nth-child(1),
.plan-table td:nth-child(1){
	padding-left: 3px;
	text-align: left;
}
.index-plan caption {
	width: 100%;
	height: 37px;
	text-align: left;
	padding-left: 10px;
	font-family: MicrosoftYaHei;
	font-size: 18px;
	letter-spacing: 1px;
	color: #111111;
	position: relative;
	padding-top: 12px;
}

.index-plan caption .calendar-box {
	background-color: #ffffff;
	border-radius: 3px;
	border: solid 1px #f5f5f5;
	font-family: MicrosoftYaHei;
	font-size: 12px;
	letter-spacing: 0px;
	color: #888888;
	margin-left: 39px;
}

.index-plan caption li {
	float: left;
	font-family: MicrosoftYaHeiLight;
	font-size: 12px;
	letter-spacing: 0px;
	color: #555555;
	margin-right: 20px;
}

.index-plan caption li label {
	display: inline-block;
	width: 8px;
	height: 8px;
	border-radius: 4px;
	margin-right: 10px;
}

.index-plan caption li:nth-child(1) label {
	background-color: #b8b3e2;
}

.index-plan caption li:nth-child(2) label {
	background-color: #847ccf;
}

.index-plan caption li:nth-child(3) label {
	background-color: #df94e0;
}

.index-plan caption ul {
	margin: 0 auto;
	width: 250px;
	height: 20px;
	position: absolute;
	left: 0;
	right: 0;
	top: 17px;
}

.index-plan caption div.right {
	float: right;
	margin-right: 11px;
}

.index-plan caption div.right span {
	font-family: MicrosoftYaHei;
	font-size: 16px;
	letter-spacing: 0px;
	color: #7b74c7;
	margin-left: 35px;
	cursor: pointer;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

.state-day {
	width: 30px;
	height: 30px;
}

.state-day.all-use:after,
.state-day.sub-use:after,
.state-day.no-use:after,
.state-day.alr-use:after {
	content: '';
	display: inline-block;
	position: absolute;
	top: auto;
	margin: auto;
	margin-top: -11px;
	width: 100%;
	height: 22px;
	padding: 0 1px;
	
}
/*未立项*/
.state-day.sub-use:after{
	background-color: #847ccf;
}
/*已立项*/
.state-day.all-use:after {
	background-color: #b8b3e2;
}
/*已运行*/
.state-day.no-use:after {
	background-color: #df94e0;
}
/*已结项*/
.state-day.alr-use:after {
	background-color: #f59700;
}

.state-day.all-use.start-state:after,
.state-day.sub-use.start-state:after,
.state-day.no-use.start-state:after,
.state-day.alr-use.start-state:after {
	-webkit-border-radius: 3px 0 0 3px;
	-moz-border-radius: 3px 0 0 3px;
	border-radius: 3px 0 0 3px;
}

.state-day.all-use.end-state:after,
.state-day.sub-use.end-state:after,
.state-day.no-use.end-state:after,
.state-day.alr-use.end-state:after {
	-webkit-border-radius: 0 5px 5px 0;
	-moz-border-radius: 0 5px 5px 0;
	border-radius: 0 5px 5px 0;
}

.state-day.all-use.end-state.start-state:after,
.state-day.sub-use.end-state.start-state:after,
.state-day.no-use.end-state.start-state:after,
.state-day.alr-use.end-state.start-state:after {
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
}
.sun-head {
	background: url(../../../resources/images/img/sun-icon.png) no-repeat;
}

.sat-head {
	background: url(../../../resources/images/img/sat-icon.png) no-repeat;
}
.sat{
	background: url(../../../resources/images/img/sat.png) no-repeat;
}
.sun{
	background: url(../../../resources/images/img/sun.png) no-repeat;
}
/**/

.index-ready {
	border-top: none;
}

.index-ready caption {
	height: 40px;
}

.index-ready caption span,
.index-scend caption span {
	float: right;
	font-family: MicrosoftYaHei;
	font-size: 16px;
	letter-spacing: 0px;
	color: #7b74c7;
}

.index-ready th:nth-child(1),
.index-ready td:nth-child(1) {
	padding: 0;
}
