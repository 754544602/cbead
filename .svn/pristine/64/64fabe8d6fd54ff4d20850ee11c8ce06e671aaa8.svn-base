.select-title{
	font-size: 28px;
	padding: 20px
}
.budget-radio{
	margin-left: 20px;
	margin-bottom: 20px;
}
.budget-organ{
	margin-left: 20px;
	margin-bottom: 20px;
}
.margin-label{
	margin-left: 20px;
}
.text-overflow{
	max-width: 300px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.level-one-div , .level-two-div , .level-three-div{
	margin-top: 10px;
}
.bold-div{
	padding-left: 20px;
	padding-top: 10px;
	text-align: left;
}
.button-box {
    padding: 10px 0;
    text-align: center;
}
.button-box button {
    height: 40px;
    line-height: 40px;
    padding: 0 30px;
    font-size: 18px;
    border-radius: 2px;
    cursor: pointer;
    outline: none;
}
.relevance-pop {
    width: 570px;
}
.yt-edit-alert .model-bottom-btn {
    text-align: center;
}
.relevance-pop .role-left-list {
	padding-top: 5px;
    width: 195px;
    padding-left: 5px;
    height: 355px;
    border: 1px solid #999;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
}
.relevance-pop .role-right-list {
    width: 190px;
    height: 350px;
    border: 1px solid #999;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    overflow: auto;
    padding: 5px;
}
.cont-edit-test table tr td {
    padding-bottom: 20px;
}
.relevance-pop .set-role-button .d-left, .relevance-pop .set-role-button .d-right {
    margin-top: 0;
}
.relevance-pop .set-role-button li button {
    margin-bottom: 18px;
}
.set-role-button button {
    width: 100px;
    height: 24px;
    border: none;
    margin: 0 5px;
    outline: none;
    cursor: pointer;
}
.model-bottom-btn button{
    margin-bottom: 10px;
}
