body {
    background: #f3f3f3;
    color: #333;
    font-size: 14px;
    font-family: Microsoft YaHei,Verdana, Arial, Helvetica, sans-serif;
}
#budgetReport,#proCostUsageQuery{
	margin: 5px;
    min-width: 1140px ;
    border: 1px solid #DFE6F3;
    background-color: #FFFFFF;
    min-height: 825px;
}


.plan-list-model {
	background: #FFFFFF;
}
.nice-select {
    display: inline-block;
    float: none;
    width: 200px;
    vertical-align: middle;
}

/*æ–°*/

.yt-table {
	table-layout: auto;
}

.page-info {
	position: relative;
}
