 */

.model-valid-sty {
	left: 0;
	top: 30px;
}
#lookReimBtn{
	margin-right: 55px;
}
.text-r{
	text-align: right;
}

.db{
	display: block;
}
.dn{
	display: none;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.tl{
	text-align: left;
}
.tr{
	text-align: right;
}
.dib{
	display: inline-block !important;
}
.nice-select {
	display: inline-block;
	float: none;
	vertical-align: middle;
}
.file-up-div{
	display: inline-block;
	vertical-align: middle;
}

.file-name-list{
	margin-left: 10px;
    display: inline-block;
    width: 60%;
    vertical-align: text-top;
    white-space: normal;
    word-wrap: break-word;
    color: #384495;
}

.file-name-list .file-name {
    margin-left: 10px;
    cursor: pointer;
}
.cont-file{
    position: absolute;
    top: 0;
    left: 0;
    z-index: 100;
    height: 30px;
    width: 70px;
    font-size: 19px;
    overflow: hidden;
    opacity: 0;
    filter: alpha(opacity=0);
}
.file-box{
	display: block !important;
	padding-left: 120px;
	
}
.file-box .li-div{
	position: relative;
	display: block !important;
	padding:5px 10px;
	color: #384495;
}
.file-box .del-file{
	width: 15px;
    height: 15px;
	margin-left: 10px;
	margin-bottom: 2px;
	vertical-align: middle;
	cursor: pointer;
}