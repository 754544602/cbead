/***
 * 
 * 
 * 
 * 费用明细弹出框
 * 
 * 
 */

.cost-detail-info-model {
	position: fixed;
	top: 0;
	left: 0;
	z-index: 10001;
	width: 693px;
	display: none;
}

.cost-detail-info-model .traffic-form,
.cost-detail-info-model .hotel-form,
.cost-detail-info-model .other-form {
	padding: 20px 20px 0px 20px;
	display: none;
}

.cost-detail-info-model .model-title img {
	margin-bottom: 3px;
}

.cost-detail-info-model button {
	font-size: 16px;
	margin-right: 30px;
}

.cost-detail-info-model table {
	width: 100%;
	table-layout: fixed;
}

.cost-type-tab {
	height: 35px;
	background: #EFF3F6;
}

.cost-type-tab ul li {
	float: left;
	display: inline;
	width: 160px;
	height: 34px;
	line-height: 34px;
	text-align: center;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-bottom: 1px solid #DFE6F3;
	color: #495295;
	cursor: pointer;
}

.tab-check {
	border-bottom: 1px solid #FFFFFF !important;
	background: #FFFFFF;
	color: #384495 !important;
}

.traffic-tab {
	width: 165px;
}

.hotel-tab,
.other-tab {
	width: 100px;
}

.time-input {
	width: 111px;
}

.model-busi-trip-user-sel {
	width: 83px;
}

.address-input {
	width: 77px;
	cursor: pointer;
}

#addres-icon {
	cursor: pointer;
}

.vehicle-sel,
.vehicle-two-sel {
	width: 83px;
}

#vehicleTwoDiv {
	float: left;
	display: inline;
	padding-left: 38px;
	display: none;
}

.city-cost-input {
	padding-right: 5px;
	margin-right: 5px;
	width: 122px;
	text-align: right;
}

.model-content textarea {
	width: 562px;
	height: 78px;
}

.traffic-form table tr td,
.hotel-form table tr td,
.other-form table tr td {
	padding-bottom: 20px;
}

#addres-icon {
	vertical-align: middle;
	margin: 0px 8px 3px 8px;
}

.model-bottom-btn {
	text-align: center;
}

#cost-apply-model .model-content {
	padding-bottom: 20px;
}

.calendar {
	z-index: 100000 !important;
}

.hotel-trip-user-sel {
	width: 91px;
}

.yt-numberInput {
	width: 54px;
}

.hotel-addres-sel {
	width: 154px;
}

.hotel-money {
	width: 109px;
	margin-right: 5px;
}

.cost-detail-money {
	padding-right: 5px;
	margin-right: 5px;
	text-align: right;
}

.yt-numberInput-box {
	width: 80px;
}

.cost-type-sel {
	width: 120px;
}

.other-money {
	width: 109px;
}

#replacementTab th {
	height: 35px;
	line-height: 35px;
	background: #475c6d;
	color: #FFFFFF;
}

#replacementTab tr td {
	height: 38px;
	line-height: 37px;
	padding: 2px 4px;
	text-align: right;
}

#replacementTab tr td,
#replacementTab th {
	border: 1px solid #DFE6F3;
}

