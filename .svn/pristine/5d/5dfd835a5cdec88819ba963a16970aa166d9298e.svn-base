}


/**
 * 
 * 费用申请样式
 * 
 */

.cost-list-title {
	padding-left: 18px;
	font-size: 16px;
	height: 35px;
	line-height: 35px;
	background: #EFF3F6;
	border: 1px solid #D6E6F3;
	border-left: 0px;
	border-right: 0px;
}

.operate-update {
	margin-right: 23px;
}

.operate-update img,
