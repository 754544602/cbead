}

.total-last-tr td {
	height: 30px;
}

.font-right {
	text-align: right !important;
}

.cost-apply-model,
.flow-approve-model {
	display: none;
}

.cost-list-model table {
	margin-bottom: 15px;
}

.cost-total-rmb {
	padding: 20px 20px 18px 16px;
